    }
    modifier onlyIfCanWidthdrawReferral() {
        require(canWidthdrawReferral == true,"it is not time to withdraw");
        _;
    }

    // Constants 
    uint256 public constant NFT_SALE_AMOUNT = 2.5 * 1 ether;
    uint16 public constant NFT_MAX_PURCHASE = 3;
    uint16 public constant REFERRAL_CUT = 10;

    // Public values
    mapping(address => uint16) public purchased;
    mapping(address => uint256) public referralQty;
    address owner;
    uint256 public totalPurchased;
    bool public canWidthdrawReferral;
    
    address[] private purchesedList;

    // Public functions
    function depositFunds(uint16 qty, address referralAddress) public payable {
        require(qty <= NFT_MAX_PURCHASE, "Can not purchase that many NFTs");
        uint256 amount = NFT_SALE_AMOUNT * qty;
        require(msg.value >= amount, "Sent wrong amount to buy");

        purchased[msg.sender] += qty;
        referralQty[referralAddress] += qty;
        require(purchased[msg.sender] <= NFT_MAX_PURCHASE, "Bought too many");

        purchesedList.push(msg.sender);
        totalPurchased += qty;

        emit BuyWhitelist();
    }

    function checkReferralQty(address referralAddress) public view returns(uint256 qty) {
        return referralQty[referralAddress];
    }

    // function whoIsOwner() public view returns(address) {
    //     return owner;
    // }

    function withdrawReferralFees() public onlyIfCanWidthdrawReferral {
        address referralAddress = msg.sender;
        require(referralQty[referralAddress] > 0, "You have nothing to withdraw");
        uint256 amount = referralQty[referralAddress] * NFT_SALE_AMOUNT * REFERRAL_CUT / 100;

        // clear out old referrals
        referralQty[referralAddress] = 0;
        
        (bool sent,) = referralAddress.call{value: amount}("");
        require(sent, "Failed to send Ether");
    }


    // Admin functions
    function withdrawFunds(uint256 amount, address withdrawAddress) public onlyAdmin {
        // Call returns a boolean value indicating success or failure.
        // This is the current recommended method to use.
        (bool sent,) = withdrawAddress.call{value: amount}("");
        require(sent, "Failed to send Ether");
    }

    function canWithdraw(bool canWidthdraw) public onlyAdmin {
        canWidthdrawReferral = canWidthdraw;
    }

    function getAllPurchasedAddress() external view onlyAdmin returns (address[] memory) {
        return purchesedList;
    }
   

}


}
