    // The first free question will send minimumBid to the receiver.
    function createFreeQuestion(uint256 userSocialId_,
                            uint256 socialNetworkId_,
                            uint256 timelock_,
                            string memory questionLink_) 
                external
                onlySocialNetworkExists(socialNetworkId_)
                onlyValidTimeLock(timelock_)
                whenNotPaused()
                nonReentrant
                returns (bytes32){
                require(freeQuestions[_msgSender()] == 0, AmaFansLib.FIRST_QUESTION_EXHAUSTED);
                require(address(this).balance > minimumBid, AmaFansLib.WE_ARE_POOR);
