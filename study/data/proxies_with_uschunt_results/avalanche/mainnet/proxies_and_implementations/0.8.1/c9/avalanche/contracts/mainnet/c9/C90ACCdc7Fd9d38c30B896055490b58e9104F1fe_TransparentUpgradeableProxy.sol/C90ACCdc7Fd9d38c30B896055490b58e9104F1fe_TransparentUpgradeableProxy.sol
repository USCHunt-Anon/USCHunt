    function _createQuestion(
                            uint256 userSocialId_,
                            address _sender,
                            address _recipient,
                            uint256 _value,
                            uint256 _timelock,
                            uint256 _socialNetworkId,
                            string memory _questionLink)
                            internal 
                            returns (bytes32){
        


        bytes32 _questionId = AmaFansLib.createQuestionId(_questionLink, _sender);

        bytes32[] memory _tipIds = new bytes32[](0);

        questions[_questionId] = AmaFansLib.Question(
                                                    _questionLink,
                                                    "",
                                                    _value,
                                                    _value,
                                                    _sender,
                                                    _recipient,
                                                    _tipIds,
                                                    block.timestamp,
                                                    _timelock,      
                                                    AmaFansLib.State.ACTIVE, 0, _socialNetworkId);
        users[_sender].questionIds.push(_questionId);
        questionIdToSocialId[_questionId] = userSocialId_;
        emit QuestionCreated(userSocialId_, _questionId, _sender, _value, _timelock, _questionLink,  _socialNetworkId);                                
        return _questionId;
    }

    //@notice This is a general method to use when a user wants to answer an question sent to her.
    function createAnswer(
                        bytes32 questionId_,
                        uint256 socialNetworkId_,
                        string memory answerLink_,
                        string memory tokenURI_)
                        external
                        onlySocialNetworkExists(socialNetworkId_)
                        onlyValidQuestionId(questionId_)
                         {
        uint256  userSocialId_ = questionIdToSocialId[questionId_];
        address socialNetworkVerificationAddress = socialNetworkVerification[socialNetworkId_];
        (, , , ,uint256 _userSocialId,) = IAmaSocialNetworkVerification(socialNetworkVerificationAddress).userDetails(_msgSender());
        require(userSocialId_ == _userSocialId, AmaFansLib.INVALID_QUESTION_OWNER);

        // require(_hash == keccak256(abi.encodePacked(twitterUsername)) ||
        //     _hash == keccak256(abi.encodePacked(domainName)), AmaFansLib.INVALID_QUESTION_OWNER);
        (uint256 _tokenId, uint256 value) = _createAnswer(questionId_, answerLink_, tokenURI_, socialNetworkId_, _msgSender());
        emit QuestionAnswered(
                    questionId_,
                    questions[questionId_].createdBy,
                    _msgSender(),
                    _tokenId,
                    value);
            }

    /// @notice When the use to who the question has been asked writes 
    //and answer and claim the value attached.
    /// @dev Returns only a fixed number.
    /// @param questionId the id of the question
    /// @param answerLink the ipfs link of the answer
    /// @param tokenURI the storage metadata_uri on which the metadata is stored
    function _createAnswer(bytes32 questionId,
                        string memory answerLink,
                        string memory tokenURI,
                        uint256 _socialNetworkId,
                        address sender)
                        internal
                        returns (uint256, uint256){

        AmaFansLib.Question storage question = questions[questionId];
        
        require(question.socialNetworkId == _socialNetworkId, AmaFansLib.SOCIALNETWORK_MISMATCH);
        AmaFansLib.questionValidityForAnswer(question);

        uint256 tokenId = mintNFTforAnswer(question.createdBy, tokenURI);
        
        AmaFansLib.questionChangesOnAnswer(question, tokenId, answerLink, sender);
        
        //Crediting _msgSender() with value attached with the question afterDeducting fee
        uint256 afterDeduction = _valueDeduction(question.totalValue);
        balances[_msgSender()] += afterDeduction;
           
        // AmaFansReputationLib.answerCreated(sReputations, _msgSender(), afterDeduction);
        emit AmountReceived(_msgSender(), afterDeduction);
        return (tokenId, afterDeduction);
    }

    function mintNFTforAnswer(address owner, string memory _tokenURI) 
        private 
        returns(uint256){
        try nftContract.mint(owner, _tokenURI) returns (uint256 tokenId ) {
            return tokenId;
        } catch Error(string memory reason) {
            revert(reason);
        }
    }

    function createTip(bytes32 questionId)
        external
        payable
        onlyValidQuestionId(questionId){
        AmaFansLib.Question storage question = questions[questionId];
        AmaFansLib.questionValidityForNewTip(question);     

        bytes32 tipId = AmaFansLib.newTip(tips, questionId, _msgSender(), msg.value);

        AmaFansLib.questionChangesOnTip(question, tipId, msg.value);

        AmaFansLib.User storage user = users[_msgSender()];
        AmaFansLib.userChangesOnTip(user, tipId);

        emit TipCreated(questionId, tipId, _msgSender(),  msg.value);
        }

    function claimBackQuestionValue(bytes32 questionId) 
            external 
            whenNotPaused()
            onlyValidQuestionId(questionId)
            onlyQuestionOwner(questionId)
        {
        AmaFansLib.Question storage question = questions[questionId];
       
        AmaFansLib.claimBackQuestionValue(question);
        balances[_msgSender()] += question.initialValue;
        emit AmountReceived(_msgSender(), question.initialValue);
        emit QuestionValueClaimed(questionId, _msgSender(), question.initialValue);

        }
   
    function claimBackTipValue(bytes32 tipId) 
            external
            whenNotPaused()
            onlyValidTipId(tipId)
            onlyTipOwner(tipId)
        {
        AmaFansLib.Tip storage tip = tips[tipId];
        AmaFansLib.Question storage question = questions[tip.questionId];
        AmaFansLib.claimBackTipValue(tip, question);
        balances[_msgSender()] += tip.value;
        emit AmountReceived(_msgSender(), tip.value);
        emit  TipValueClaimed(tip.questionId, tipId, _msgSender(), tip.value);

        }
    function _valueDeduction(uint256 _value) private returns (uint256) {
        uint256 deduction = (_value * feeNumerator) / _feeDenominator();
        balances[feeCollector] += deduction;
        return (_value - deduction);
            
    }
    
    function balance() 
        external 
        view 
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (uint256){
        return address(this).balance;
    }

    function userBalance() 
        external 
        view  
        returns (uint256){
        return balances[_msgSender()];
    }
    

    function withdraw(uint256 _amount) 
        external
        whenNotPaused()
        nonReentrant 
        {
        // if (_amount >= balances[_msgSender()]) revert AmaFansLib.InSufficientFunds();
        require(_amount <= balances[_msgSender()], AmaFansLib.INSUFFICIENT_FUNDS);

        balances[_msgSender()] -= _amount;
        // payable(_msgSender()).transfer(_amount);
        Address.sendValue(payable(_msgSender()), _amount);
        emit Withdraw(_msgSender(), _amount);
            }
    
    function getQuestionTipIds(bytes32 _questionId,
                uint256 skip, 
                uint256 limit) 
        external 
        view
        onlyValidQuestionId(_questionId)
       returns (bytes32[] memory values, uint256){
        if (limit > questions[_questionId].tipIds.length - skip) {
                limit = questions[_questionId].tipIds.length - skip;
            }
        values = new bytes32[](limit);
