contract Payments is Initializable, OwnableUpgradeable {
    enum PaymentReason {
        Tip,
        Subscription
    }

    IERC20 public cofToken;
    /// @dev Address to which payment taxes are sent
    address public taxAddress;

    /// @dev The amount of COF that an address can withdraw
    mapping(address => uint256) private _cofBalanceByAddress;
    /// @dev If the address was referred by someone, it is stored here
    mapping(address => address) public referrerByAddress;

    /// @dev Percentage of payments that is sent to the tax address
    uint8 public taxPercent;
    /// @dev Percentage of payments that is sent to the referrer
    uint8 public referrerPercent;

    event Payment(
        address indexed from,
        address indexed to,
        PaymentReason indexed reason,
        uint256 amount
    );

    function initialize(IERC20 _cofToken, address _taxAddress) external initializer {
        __Ownable_init_unchained();
        cofToken = _cofToken;
        taxAddress = _taxAddress;
        taxPercent = 4;
    }

    /// @dev Pay COF tokens to an address
    /// @param to The address to pay to
    /// @param amount The amount of COF tokens to pay
    function payCof(
        address to,
        uint256 amount,
        PaymentReason reason
    ) external {
        require(to != address(0), "recipient cannot be zero address");
        require(amount > 0, "amount cannot be zero");

        address recipientReferrer = referrerByAddress[to];
        uint256 taxAmount;

        // Pay to referrer of taxAddress
        if (recipientReferrer != address(0)) {
            uint256 referrerAmount = amount * referrerPercent / 100;
            _cofBalanceByAddress[recipientReferrer] += referrerAmount;
            _cofBalanceByAddress[to] += amount - referrerAmount;
        } else {
            taxAmount = (amount * taxPercent) / 100;
            _cofBalanceByAddress[to] += amount - taxAmount;
        }

        cofToken.transferFrom(msg.sender, address(this), amount);

        if (taxAmount > 0) {
            cofToken.transfer(taxAddress, taxAmount);
        }


        emit Payment(msg.sender, to, reason, amount);
    }

    /// @dev Sends all the COF that has been paid to the sender
    function withdraw() external {
        uint256 balance = _cofBalanceByAddress[msg.sender];
        require(balance > 0, "no COF balance to withdraw");

        _cofBalanceByAddress[msg.sender] = 0;

        cofToken.transfer(msg.sender, balance);
    }

    /// @dev Sets the senders referrer
    function setReferrer(address referrer) external {
        require(referrer != address(0), "referrer cannot be zero address");
        referrerByAddress[msg.sender] = referrer;
    }
    /// @dev Sets the wallet that receives payment taxes
    function setTaxWallet(address _taxAddress) external onlyOwner {
        taxAddress = _taxAddress;
    }

    /// @dev Returns the amount of COF that can be withdrawn by the sender
    function getBalance() external view returns (uint256) {
        return _cofBalanceByAddress[msg.sender];
    }

}


