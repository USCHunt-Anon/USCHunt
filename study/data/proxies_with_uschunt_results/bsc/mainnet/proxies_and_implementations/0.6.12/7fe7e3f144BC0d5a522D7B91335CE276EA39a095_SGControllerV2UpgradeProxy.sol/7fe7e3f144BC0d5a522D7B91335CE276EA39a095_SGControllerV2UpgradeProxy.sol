    function swapAndLiquify(uint256 _pid, address[] memory routerPath) public onlyOwner {
        PoolInfo storage pool = poolInfo[_pid];

        //do stake 0
        uint256 beforeCake = cake.balanceOf(address(this));

        pool.lpToken.approve(address(cakeFarmMasterChef), uint256(- 1));
        cakeFarmMasterChef.deposit(pidMap[_pid], 0);

        uint256 damount;
        (damount,) = cakeFarmMasterChef.userInfo(pidMap[_pid], address(this));
        poolExchangeRate[_pid] = damount.mul(1e18).div(pool.lpAmount);

        pool.cakeLeft = pool.cakeLeft.add(cake.balanceOf(address(this)).sub(beforeCake));
        //end

        address token0;
        address token1;
        address lastAddress = routerPath[routerPath.length - 1];

        if (IUniswapV2Pair(address(pool.lpToken)).token0() == lastAddress) {
            token0 = IUniswapV2Pair(address(pool.lpToken)).token0();
            token1 = IUniswapV2Pair(address(pool.lpToken)).token1();
        } else if (IUniswapV2Pair(address(pool.lpToken)).token1() == lastAddress) {
            token0 = IUniswapV2Pair(address(pool.lpToken)).token1();
            token1 = IUniswapV2Pair(address(pool.lpToken)).token0();
        } else {
            revert('path error');
        }

        IERC20(token0).approve(address(router), uint256(- 1));
        IERC20(token1).approve(address(router), uint256(- 1));
        cake.approve(address(router), uint256(- 1));

        uint256 fee = pool.cakeLeft.mul(40).div(100);
        cake.transfer(owner(), fee);
        pool.cakeLeft = pool.cakeLeft.sub(fee);
        router.swapExactTokensForTokens(pool.cakeLeft, 0, routerPath, address(this), now);
        pool.cakeLeft = 0;

        uint256 token0Amount = IERC20(token0).balanceOf(address(this));

        address[] memory tmpPath = new address[](2);
        tmpPath[0] = token0;
        tmpPath[1] = token1;
        router.swapExactTokensForTokens(token0Amount.div(2), 0, tmpPath, address(this), now);

        token0Amount = IERC20(token0).balanceOf(address(this));
        uint256 token1Amount = IERC20(token1).balanceOf(address(this));

        router.addLiquidity(token0, token1, token0Amount, token1Amount, 0, 0, address(this), now);
        depositInterest(_pid);

        IERC20(token0).transfer(owner(), IERC20(token0).balanceOf(address(this)));
        IERC20(token1).transfer(owner(), IERC20(token1).balanceOf(address(this)));
    }
