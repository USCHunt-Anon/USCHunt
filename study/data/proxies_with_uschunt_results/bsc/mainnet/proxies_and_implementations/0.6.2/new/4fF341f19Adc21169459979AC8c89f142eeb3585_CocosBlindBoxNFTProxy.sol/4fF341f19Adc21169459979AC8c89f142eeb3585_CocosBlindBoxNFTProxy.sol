// File: contracts/blindbox/CocosBlindBoxNFT.sol

pragma solidity ^0.6.0;









contract CocosBlindBoxNFT is Governance, ERC721, ICocosBlindBoxNFT, IERC2981, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    // Flag of initialize data
    bool private initialized;

    // for minters
    mapping(address => bool) public _minters;
    uint256 public _tokenID;
    
    uint256 public _gasFeeRate = 10000;
    uint256 public _baseRate = 100000;
    address payable public _teamAddress;

    ICocosVipBook public _cocosVipBook;

    mapping(uint256 => uint256) public _rules;

    struct BoxInfo {
        address asset; // erc20
        uint256 price; // price
        uint256 total; // sell amount
        uint256 userMinLimit; // one can buy min
        uint256 userMaxLimit; // one can buy max
        uint256 startTime; // start 
        uint256 endTime; // end
        uint256 sold; // 
        uint256 soldPrice; // 
    }

    // bindbox
    // the project user bought
    mapping(uint256 => mapping(address => uint256)) public _boxBoughts;
    
    // the project price
    mapping(uint256 => BoxInfo) public _projBoxInfos;

    /// @dev
    /// bytes4(keccak256("royaltyInfo(uint256)")) == 0xcef6d368
    /// bytes4(keccak256("onRoyaltiesReceived(address,address,uint256,address,uint256,bytes32)")) == 0xe8cb9d99
    /// bytes4(0xcef6d368) ^ bytes4(0xe8cb9d99) == 0x263d4ef1
    bytes4 private constant _INTERFACE_ID_ERC721ROYALTIES = 0x263d4ef1;

    /// @notice Called to return both the creator's address and the royalty percentage
    /// @param _tokenId - the NFT asset queried for royalty information
    /// @return receiver - address of who should be sent the royalty payment
    /// @return amount - a percentage calculated as a fixed point
    ///         with a scaling factor of 100000 (5 decimals), such that
    ///         100% would be the value 10000000, as 10000000/100000 = 100.
    ///         1% would be the value 100000, as 100000/100000 = 1
    struct RoyaltyInfo {
        address creator;
        uint256 amount;
    }

    uint256 public _defultRoyalty = 0; // 1000000; // 10%
    mapping(uint256 => RoyaltyInfo) private _royaltyInfos;

    event CreatorChanged(uint256 indexed _id, address indexed _creator);
    event RoyaltyChanged(uint256 indexed _id, uint256 indexed _royalty);
    event DefultRoyaltyChanged(uint256 _royalty);

    event CocosBlindBoxNFTAdd(address indexed account, uint256 indexed id, uint256 indexed rule);
    event CocosBlindBoxNFTBurn(uint256 indexed id);
    event BuyBlindBoxNFT(
        address indexed sender, 
        uint256 indexed proj, 
        uint256 tokenID,
        address asset,
        uint256 price,
        uint256 sold);

    constructor() public ERC721("BlindBoxNFT", "BlindBoxNFT.COCOS") {
        // Royalties interface 
        _registerInterface(_INTERFACE_ID_ERC721ROYALTIES);
        _setBaseURI("http://tests.cocosbcx.io/api/v1/metadata/kovan/");
    }

    // --- Init ---
    function initialize(ICocosVipBook cocosVipBook, address payable teamAddress) public {
        require(!initialized, "initialize: Already initialized!");
        _governance = msg.sender;
        
        _registerInterface(0x263d4ef1);
        _setBaseURI("http://tests.cocosbcx.io/api/v1/metadata/kovan/");
        _cocosVipBook = cocosVipBook;
        
        _minters[address(this)] = true;
        
        _gasFeeRate = 10000;
        _baseRate = 100000;
        _teamAddress = teamAddress;
        initialized = true;
    }

    // sell
    function buy(uint256 proj, uint256 amount) public payable {
        require(!Address.isContract(msg.sender), "CocosBlindBoxNFT: can't call");
        require(_cocosVipBook.hasRegistered(msg.sender), "CocosBlindBoxNFT: must vip");

        BoxInfo storage boxInfo = _projBoxInfos[proj];
        require(boxInfo.startTime > 0, "CocosBlindBoxNFT: BlindBox not exist");
        require(block.timestamp > boxInfo.startTime, "CocosBlindBoxNFT: not start");
        require(block.timestamp < boxInfo.endTime, "CocosBlindBoxNFT: have end");
        uint256 sold = boxInfo.sold;
        require(sold < boxInfo.total, "CocosBlindBoxNFT: sold out");
        
        uint256 userBought = _boxBoughts[proj][msg.sender];
        if (userBought == 0) {
            require(amount >= boxInfo.userMinLimit, "CocosBlindBoxNFT: Minimum limit not reached");
        }
        require(userBought < boxInfo.userMaxLimit, "CocosBlindBoxNFT: bought exceeds limit");
        
        uint256 canBuyAmount = boxInfo.userMaxLimit.sub(userBought);
        uint256 poolSurplus = boxInfo.total.sub(sold);
        if (canBuyAmount > poolSurplus) {
            canBuyAmount = poolSurplus;
        }
        
        if (boxInfo.asset == address(0)) { // eth
            require(msg.value >= boxInfo.price, "CocosBlindBoxNFT: price too low");

            uint256 buyAmount = msg.value.div(boxInfo.price);
            
            if (buyAmount > canBuyAmount) {
                uint256 totalPrice = canBuyAmount.mul(boxInfo.price);
                uint256 returnBack = msg.value.sub(totalPrice);
                if(returnBack > 0) {
                    msg.sender.transfer(returnBack);
                }
                mint_(proj, msg.sender, canBuyAmount);
            } else {
                uint256 totalPrice = buyAmount.mul(boxInfo.price);
                uint256 returnBack = msg.value.sub(totalPrice);
                if(returnBack > 0) {
                    msg.sender.transfer(returnBack);
                }
                mint_(proj, msg.sender, buyAmount);
            }

        } else {
            if (canBuyAmount > amount) {
            
                uint256 totalPrice = amount.mul(boxInfo.price);
                IERC20(boxInfo.asset).safeTransferFrom(msg.sender, address(this), totalPrice);
                mint_(proj, msg.sender, amount);
            } else {
                
                uint256 totalPrice = canBuyAmount.mul(boxInfo.price);
                IERC20(boxInfo.asset).safeTransferFrom(msg.sender, address(this), totalPrice);
                mint_(proj, msg.sender, canBuyAmount);
            }
        }
        
        emit BuyBlindBoxNFT(
            msg.sender,
            proj,
            _tokenID,
            boxInfo.asset,
            boxInfo.price,
            boxInfo.sold
        );
    }

    function mint_(uint256 proj, address sender, uint256 amount) internal {
        
        BoxInfo storage boxInfo = _projBoxInfos[proj];
        boxInfo.sold = boxInfo.sold.add(amount);
        uint256 totalPrice = boxInfo.price.mul(amount);
        boxInfo.soldPrice = boxInfo.soldPrice.add(totalPrice);

        _boxBoughts[proj][sender] = _boxBoughts[proj][sender].add(amount);

        for (uint256 index = 0; index < amount; index++) {
            _tokenID++;
            this.mint(msg.sender, _tokenID, proj);
        }
    }

    function mint(
        address account,
        uint256 id,
        uint256 rule)
    external override nonReentrant {
        require(_minters[msg.sender], "CocosBlindBoxNFT: INVALID_MINTER.");
        require(id > 0, "CocosBlindBoxNFT: INVALID_ID.");
        _mint(account, id);

        _royaltyInfos[id].creator = account;
        _royaltyInfos[id].amount = _defultRoyalty;
        _rules[id] = rule;

        emit CocosBlindBoxNFTAdd(
            account,
            id,
            rule);
    }
    
    function burn(uint256 _id) external override nonReentrant {
        require(_isApprovedOrOwner(_msgSender(), _id), "CocosBlindBoxNFT: burn caller is not owner nor approved");
        _burn(_id);
        _royaltyInfos[_id].creator = address(0);
        _royaltyInfos[_id].amount = 0;
        _rules[_id] = 0;
        emit CocosBlindBoxNFTBurn(_id);
    }

    // for rule
    function getRule(uint256 id) external view override returns (uint256 rule) {
        rule = _rules[id];
    }

    // for DAO
    function setTeamAddress(address payable teamAddress) public onlyGovernance {
        _teamAddress = teamAddress;
    }

    function setGasFeeRate(uint256 gasFeeRate) public onlyGovernance {
        _gasFeeRate = gasFeeRate;
    }
    
    function setURIPrefix(string memory baseURI) public onlyGovernance {
        _setBaseURI(baseURI);
    }
     
    function setTokenID(uint256 tokenID) public onlyGovernance {
        _tokenID = tokenID;
    }
    
    function setDefultRoyalty(uint256 _royalty) public onlyGovernance {
        _defultRoyalty = _royalty;
        emit DefultRoyaltyChanged(_royalty);
    }

    function addMinter(address minter) public onlyGovernance {
        _minters[minter] = true;
    }

    function removeMinter(address minter) public onlyGovernance {
        _minters[minter] = false;
    }

    function withdraw(address asset, uint256 amount, address payable addr) external onlyGovernance {
        uint256 feeAmount = amount.mul(_gasFeeRate).div(_baseRate);
        uint256 sellAmount = amount.sub(feeAmount);
        if (asset == address(0)) {
            _teamAddress.transfer(feeAmount);
            addr.transfer(sellAmount);
        } else {
            IERC20(asset).safeTransfer(_teamAddress, feeAmount);
            IERC20(asset).safeTransfer(addr, sellAmount);
        }
    }

    // set Blind Box 
    function setProjectBlindBox (
        address asset, uint256 amount,
        uint256 proj, uint256 total, uint256 userMinLimit,
        uint256 userMaxLimit, uint256 startTime, uint256 endTime) 
        public onlyGovernance {
           
           BoxInfo memory boxInfo;
           boxInfo.asset = asset;
           boxInfo.price = amount;
           boxInfo.total = total;
           boxInfo.userMinLimit = userMinLimit;
           boxInfo.userMaxLimit = userMaxLimit;
           boxInfo.startTime = startTime;
           boxInfo.endTime = endTime;
           
           _projBoxInfos[proj] = boxInfo;
    }

    function setProjectBlindBoxTime (
        uint256 proj, 
        uint256 startTime, 
        uint256 endTime) 
        public onlyGovernance {
           
           BoxInfo storage boxInfo = _projBoxInfos[proj];
           boxInfo.startTime = startTime;
           boxInfo.endTime = endTime;
           
    }

    // for royalty
    function royaltyInfo(uint256 _id) external view override returns (address receiver, uint256 amount) {
        receiver = _royaltyInfos[_id].creator;
        amount = _royaltyInfos[_id].amount;
    }

    function setCreator(uint256 _id, address _to) public onlyGovernance {
        require(_to != address(0),"CocosBlindBoxNFT: INVALID_ADDRESS.");
        _royaltyInfos[_id].creator = _to;
        emit CreatorChanged(_id, _to);
    }

    function setRoyalty(uint256 _id, uint256 _amount) public onlyGovernance {
        _royaltyInfos[_id].amount = _amount;
        emit RoyaltyChanged(_id, _amount);
    }

    function onRoyaltiesReceived(address _royaltyRecipient, address _buyer, uint256 _tokenId, address _tokenPaid, uint256 _amount, bytes32 _metadata) external override returns (bytes4) {
        emit RoyaltiesReceived(_royaltyRecipient, _buyer, _tokenId, _tokenPaid, _amount, _metadata);    
        return bytes4(keccak256("onRoyaltiesReceived(address,address,uint256,address,uint256,bytes32)"));
    }

     /**
     * @dev See {IERC721Metadata-name}.
     */
    function name() public view virtual override returns (string memory) {
        return "BlindBoxNFT";
    }

    /**
     * @dev See {IERC721Metadata-symbol}.
     */
    function symbol() public view virtual override returns (string memory) {
        return "BlindBoxNFT.COCOS";
    }

    fallback() external {}
    
    receive() payable external {}

}