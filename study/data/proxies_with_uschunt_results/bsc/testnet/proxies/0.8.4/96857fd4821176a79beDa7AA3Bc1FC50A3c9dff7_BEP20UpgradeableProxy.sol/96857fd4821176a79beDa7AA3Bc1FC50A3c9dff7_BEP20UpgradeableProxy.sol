contract Storage {
    address public _owner;
    address _previousOwner;
    uint256 _lockTime;
    
    address payable public rewardAddress = payable(0xf2C1107D223E01A8707CbeDb29fBa8D5A389FB3F); // Reward Address
    address public immutable deadAddress = 0x000000000000000000000000000000000000dEaD;
    mapping (address => uint256) public _rOwned;
    mapping (address => uint256) public _tOwned;
    mapping (address => mapping (address => uint256)) public _allowances;

    mapping (address => bool) public _isExcludedFromFee;

    mapping (address => bool) public _isExcluded;
    address[] public _excluded;
   
    uint256 public constant MAX = ~uint256(0);
    uint256 public _tTotal = 1000000000 * 10**6 * 10**9;
    uint256 public _rTotal = (MAX - (MAX % _tTotal));
    uint256 public _tFeeTotal;

    string public _name = "FireZard";
    string public _symbol = "Zard";
    uint8 public _decimals = 9;


    uint256 public _taxFee = 2;
    uint256 public _previousTaxFee = _taxFee;
    
    uint256 public _liquidityFee = 9;
    uint256 public _previousLiquidityFee = _liquidityFee;
    
    uint256 public rewardDivisor = 4;
    
    uint256 public _maxTxAmount = 3000000 * 10**6 * 10**9;
    uint256 public minimumTokensBeforeSwap = 200000 * 10**6 * 10**9; 
    uint256 public buyBackUpperLimit = 1 * 10**18;
    
    uint256 public buyBackAmountLimit = 30 * 10**18;

    IUniswapV2Router02 public immutable uniswapV2Router;
    address public immutable uniswapV2Pair;
    
    bool inSwapAndLiquify;
    bool public swapAndLiquifyEnabled = false;
    bool public buyBackEnabled = true;
    
    constructor () {
        _owner = msg.sender;
        
        _rOwned[_owner] = _rTotal;
        
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0xD99D1c33F9fC3444f8101754aBC46c52416550D1);
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;

        
        _isExcludedFromFee[_owner] = true;
        _isExcludedFromFee[address(this)] = true;
    }
}

