       __Ownable_init();

        // Total supply
        _tTotal = 1000000000000 * 10**18;
        _rTotal = (MAX - (MAX % _tTotal));

        testowner = owner();

        _name = "MK Token";
        _symbol = "MKT";
        _decimals = 18;
        _taxFee = 20;
        _previousTaxFee = _taxFee;
        _developmentFee = 10;
        _previousDevelopmentFee = _developmentFee;
        _liquidityFee = 50;
        _previousLiquidityFee = _liquidityFee;
        swapAndLiquifyEnabled = true;
        _maxTxAmount = 1000000000000 * 10**18;
        numTokensSellToAddToLiquidity = 1000000000 * 10**18; // 0.1% of total

