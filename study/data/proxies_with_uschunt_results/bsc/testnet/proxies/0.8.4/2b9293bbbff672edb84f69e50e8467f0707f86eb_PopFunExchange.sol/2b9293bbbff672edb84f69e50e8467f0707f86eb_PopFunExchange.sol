        cancelledOrFinalized[hash] = bo;
    }

    function updateNonce(address addr, uint256 nonce) inWhiteList external {
        nonces[addr] = nonce;
        emit NonceIncremented(addr, nonce);
    }

    function updateWhiteList(address addr, bool bo) onlyOwner external {
        _whiteList[addr] = bo;
    }

