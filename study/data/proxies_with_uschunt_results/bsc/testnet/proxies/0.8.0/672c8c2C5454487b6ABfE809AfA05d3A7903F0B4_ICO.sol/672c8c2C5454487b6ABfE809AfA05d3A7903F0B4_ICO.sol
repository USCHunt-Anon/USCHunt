    function supportsInterface(bytes4 id) external view returns (bool);
}

abstract contract Proxy {
    // /////////////////////// EVENTS ///////////////////////////////////////////////////////////////////////////

    event ProxyImplementationUpdated(address indexed previousImplementation, address indexed newImplementation);

    // ///////////////////// EXTERNAL ///////////////////////////////////////////////////////////////////////////

    receive() external payable virtual {
        revert("ETHER_REJECTED"); // explicit reject by default
    }

    fallback() external payable {
        _fallback();
    }

    // ///////////////////////// INTERNAL //////////////////////////////////////////////////////////////////////

    function _fallback() internal {
        // solhint-disable-next-line security/no-inline-assembly
        assembly {
            let implementationAddress := sload(0x360894a13ba1a3210667c828492db98dca3e2076cc3735a920a3ca505d382bbc)
            calldatacopy(0x0, 0x0, calldatasize())
            let success := delegatecall(gas(), implementationAddress, 0x0, calldatasize(), 0, 0)
            let retSz := returndatasize()
            returndatacopy(0, 0, retSz)
            switch success
                case 0 {
                    revert(0, retSz)
                }
                default {
                    return(0, retSz)
                }
        }
    }

    function _setImplementation(address newImplementation, bytes memory data) internal {
        address previousImplementation;
        // solhint-disable-next-line security/no-inline-assembly
        assembly {
            previousImplementation := sload(0x360894a13ba1a3210667c828492db98dca3e2076cc3735a920a3ca505d382bbc)
        }

        // solhint-disable-next-line security/no-inline-assembly
        assembly {
            sstore(0x360894a13ba1a3210667c828492db98dca3e2076cc3735a920a3ca505d382bbc, newImplementation)
        }

        emit ProxyImplementationUpdated(previousImplementation, newImplementation);

        if (data.length > 0) {
            (bool success, ) = newImplementation.delegatecall(data);
            if (!success) {
                assembly {
                    // This assembly ensure the revert contains the exact string data
                    let returnDataSize := returndatasize()
                    returndatacopy(0, 0, returnDataSize)
                    revert(0, returnDataSize)
                }
            }
        }
    }
}

contract EIP173Proxy is Proxy {
    // ////////////////////////// EVENTS ///////////////////////////////////////////////////////////////////////

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    // /////////////////////// CONSTRUCTOR //////////////////////////////////////////////////////////////////////

    constructor(
        address implementationAddress,
        address ownerAddress,
        bytes memory data
    ) payable {
        _setImplementation(implementationAddress, data);
        _setOwner(ownerAddress);
    }

    // ///////////////////// EXTERNAL ///////////////////////////////////////////////////////////////////////////

    function owner() external view returns (address) {
        return _owner();
    }

    function supportsInterface(bytes4 id) external view returns (bool) {
        if (id == 0x01ffc9a7 || id == 0x7f5828d0) {
            return true;
        }
        if (id == 0xFFFFFFFF) {
            return false;
        }

        ERC165 implementation;
        // solhint-disable-next-line security/no-inline-assembly
        assembly {
            implementation := sload(0x360894a13ba1a3210667c828492db98dca3e2076cc3735a920a3ca505d382bbc)
        }

        // Technically this is not standard compliant as ERC-165 require 30,000 gas which that call cannot ensure
        // because it is itself inside `supportsInterface` that might only get 30,000 gas.
        // In practise this is unlikely to be an issue.
        try implementation.supportsInterface(id) returns (bool support) {
            return support;
        } catch {
            return false;
        }
    }

    function transferOwnership(address newOwner) external onlyOwner {
        _setOwner(newOwner);
    }

    function upgradeTo(address newImplementation) external onlyOwner {
        _setImplementation(newImplementation, "");
    }

    function upgradeToAndCall(address newImplementation, bytes calldata data) external payable onlyOwner {
        _setImplementation(newImplementation, data);
    }

    // /////////////////////// MODIFIERS ////////////////////////////////////////////////////////////////////////

    modifier onlyOwner() {
        require(msg.sender == _owner(), "NOT_AUTHORIZED");
        _;
    }

    // ///////////////////////// INTERNAL //////////////////////////////////////////////////////////////////////

    function _owner() internal view returns (address adminAddress) {
        // solhint-disable-next-line security/no-inline-assembly
        assembly {
            adminAddress := sload(0xb53127684a568b3173ae13b9f8a6016e243e63b6e8ee1178d6a717850b5d6103)
        }
    }

    function _setOwner(address newOwner) internal {
        address previousOwner = _owner();
        // solhint-disable-next-line security/no-inline-assembly
        assembly {
            sstore(0xb53127684a568b3173ae13b9f8a6016e243e63b6e8ee1178d6a717850b5d6103, newOwner)
        }
        emit OwnershipTransferred(previousOwner, newOwner);
    }
}

contract ICO {
    EIP173Proxy private proxy;
    Sale private sale; 
    uint256 constant DECIMALES = 1 * (10 ** 18);
    uint256[] private tiersBBCN; 
    uint256[] private tiersCARBO; 
    uint256[] private limiteTotalCARBO; 
    uint256 private precioCARBO; 
    address private owner;
    address private walletParaRetornarFondos;
   	address private walletBeneficioICO;
	uint256 private fechaInicioClaimFinal;
	uint256 private fechaCierreCompraTokenFinal;
	uint8 private porcentajeBeneficioICO; 
    IERC20 private tokenCarbo;
    struct EstadoToken { 
        uint256 cantidadCompradaToken;    
    }
    mapping (address => EstadoToken) private tokens;

	/* Eventos */
    event Claim(address wallet,uint256 cantidad);
    event Comprar(address wallet,uint256 cantidad);

    constructor ( address payable addressProxy, uint256[] memory limitesBBCN, 
    uint256[] memory limitesCARBO, uint256 precioUnidadBNB, address addressCarbo,
    uint256[] memory limiteVentasPorTier, uint256 fechaInicioClaim,uint256 fechaCierreCompraToken,
	uint8 porcentajeBeneficioICOInitial,address walletBeneficio)  {
		owner = msg.sender;
		walletParaRetornarFondos = owner; 
		cambiarDireccionContratoImplementacion(addressProxy); 
        cambiarLimiteBBCN(limitesBBCN);
        cambiarLimiteCARBO(limitesCARBO);
        cambiarCantidadCarboPorUnBNB(precioUnidadBNB);
        cambiarDireccionContratoCompraICO(addressCarbo);
        cambiarLimiteVentaPorTier(limiteVentasPorTier); 
		cambiarTiempoDeVesting(fechaInicioClaim); 
		cambiarTiempoDeCompra(fechaCierreCompraToken); 
		cambiarPorcentajeBeneficioICO(porcentajeBeneficioICOInitial);
		cambiarWalletBeneficioICO(walletBeneficio);
    }

	/* Funciones Administrativas */

    modifier isOwner() {
        require(msg.sender == owner, "No eres el Owner");
        _;
    }	
	function cambiarWalletBeneficioICO(address walletBeneficio) public isOwner {
		walletBeneficioICO = walletBeneficio;
    }		
	function cambiarPorcentajeBeneficioICO(uint8 porcentajeBeneficioICOInitial) public isOwner {
		porcentajeBeneficioICO = porcentajeBeneficioICOInitial;
    }		
	function cambiarTiempoDeCompra(uint256 fechaCierreCompraToken) public isOwner {
		fechaCierreCompraTokenFinal = fechaCierreCompraToken;
    }	
    function cambiarTiempoDeVesting(uint256 fechaInicioClaim) public isOwner {
       fechaInicioClaimFinal = fechaInicioClaim; 
    }	
	function cambiarDireccionContratoImplementacion(address payable addressImplementation) public isOwner {
        proxy = EIP173Proxy(addressImplementation);  
        sale = Sale(addressImplementation); 
    }
	function cambiarDireccionContratoCompraICO(address addressCarbo) public isOwner {
    	tokenCarbo = IERC20(address(addressCarbo));
    }	
    function cambiarLimiteBBCN(uint256[] memory limitesBBCN) public isOwner {
        tiersBBCN = limitesBBCN;
    }
	function cambiarLimiteCARBO(uint256[] memory limitesCARBO) public isOwner {
		tiersCARBO =   limitesCARBO;
    }
	function cambiarCantidadCarboPorUnBNB(uint256 precioUnidadBNB) public isOwner {
         precioCARBO = precioUnidadBNB * DECIMALES;
    }
	function cambiarLimiteVentaPorTier(uint256[] memory limiteVentasPorTier) public isOwner {
        limiteTotalCARBO = limiteVentasPorTier; 
    }
	function cambioWalletParaRetornarFondos(address nuevaWalletFondos) public isOwner {
    	uint256 totalCarboEnContrato = tokenCarbo.balanceOf(address(this)); 
		require(totalCarboEnContrato <= 0, "El contrato todavia tiene fondos. No se puede cambiar la Wallet");
    	walletParaRetornarFondos = nuevaWalletFondos; 
    }	
    function cambiarOwner(address newOwner) public isOwner {
        owner = newOwner;
    }
    function obtenerOwner() public view returns (address) {
        return owner;
    }

    function comprarTokensEnVesting() external payable {
        require(msg.value > 0, "No estas comprando");
        require(compraTokensActiva(), "Pasado el tiempo de compra");
        uint256 cantidadCarboAComprar = msg.value * precioCARBO; 
        uint256 nuevoTotal = tokens[msg.sender].cantidadCompradaToken + cantidadCarboAComprar; 
        require(nuevoTotal <= maximoMontoCarboComprablePorWallet(), "Limite maximo no puedes comprar nuevos tokens");
	    uint8 numeroTier = calcularTier();
        require(limiteTotalCARBO[numeroTier - 1] > msg.value, "Quedan pocos tokens");	  
	    uint256 balance = address(this).balance;
        balance += msg.value;
        tokens[msg.sender].cantidadCompradaToken = nuevoTotal; 
        limiteTotalCARBO[numeroTier - 1] -= cantidadCarboAComprar;
        emit Comprar(msg.sender,cantidadCarboAComprar); 
    }	

    function claim() external {
        require(claimActivo(), "No se ha superado el tiempo para reclamar los token");
		require(tokens[msg.sender].cantidadCompradaToken > 0, "No posses token CARBO para reclamar");
        tokenCarbo.transfer(msg.sender, tokens[msg.sender].cantidadCompradaToken);
        tokens[msg.sender].cantidadCompradaToken = 0;   
        emit Claim(msg.sender,tokens[msg.sender].cantidadCompradaToken);
    }

    function claimBNB() external {
		require(claimActivo(), "No se ha superado el tiempo para reclamar los token");
		require(msg.sender == walletParaRetornarFondos || msg.sender == walletBeneficioICO, "No puedes reclamar Tokens");
		require(address(this).balance > 0, "El contrato no posee BNB");
		uint256 cantidadBNBContrato = address(this).balance;
		uint256 totalBNBAEnviar = 0; 
		if(msg.sender == walletParaRetornarFondos) {
		 totalBNBAEnviar = ((100 - porcentajeBeneficioICO) * cantidadBNBContrato) / 100;
		} else if(msg.sender == walletBeneficioICO) {
		 totalBNBAEnviar = (porcentajeBeneficioICO * cantidadBNBContrato) / 100;
		}
		payable(msg.sender).transfer(totalBNBAEnviar);
    }

    function claimFondos() external {
		require(msg.sender == walletParaRetornarFondos, "No eres la wallet para retirar los fondos");
     	uint256 totalCarboEnContrato = tokenCarbo.balanceOf(address(this)); 
        tokenCarbo.transfer(msg.sender,totalCarboEnContrato);
    }

	function tokensDisponiblesCarboPorTier(uint8 numeroTier) public view returns (uint256) {
		return limiteTotalCARBO[numeroTier - 1];  
    }	
	function claimActivo() public view returns (bool) {
		return block.timestamp >= fechaInicioClaimFinal; 
    }	
	function compraTokensActiva() public view returns (bool) {
		return block.timestamp <= fechaCierreCompraTokenFinal; 
    }	
	function cantidadTokensCompradosPorWallet() public view returns (uint256) {
		return tokens[msg.sender].cantidadCompradaToken; 
    }	
	function balanceCarboDelContrato() public view returns (uint256) {
		return tokenCarbo.balanceOf(address(this));
    }	
    function totalCarboDisponibleParaVentas() public view returns (uint256) {
		uint256 total = limiteTotalCARBO[0] + limiteTotalCARBO[1] + limiteTotalCARBO[2] + limiteTotalCARBO[3]; 
		return total;
    }
    function cantidadDeCarboCompraPorUsuario() public view returns (uint256) {
		uint256 tokensCompradosCarbo = tokens[msg.sender].cantidadCompradaToken; 
        uint8 numeroTier = calcularTier();
		uint256 limiteCompraPorUsuario =  tiersCARBO[numeroTier - 1]; 
		return limiteCompraPorUsuario - tokensCompradosCarbo;
    }
    function balanceDelContrato() public view returns (uint256) {
        return address(this).balance;
    }
	function getIDs() public view returns (uint256[] memory) {
        return sale.getIDs(msg.sender);
    }
    function showMyShops() public view returns (uint256) {
        uint256[] memory idsCompra = sale.getIDs(msg.sender);
        uint256 acumulador = 0;
        for (uint i = 0; i < idsCompra.length; i++) {
         (, uint256 valor) = sale.showMyShops(idsCompra[i],msg.sender);
         acumulador += valor; 
        }
        return acumulador;
    }
    function maximoMontoCarboComprablePorWallet() public view returns (uint256) {
        uint256 cantidadTokens = showMyShops(); 
        uint256 maximoMonto = 0; 
        if(cantidadTokens >= tiersBBCN[0] && cantidadTokens < tiersBBCN[1] ) {
            maximoMonto = tiersCARBO[0]; 
        } else if(cantidadTokens >= tiersBBCN[1] && cantidadTokens < tiersBBCN[2] ) {
            maximoMonto = tiersCARBO[1]; 
        } else if(cantidadTokens >= tiersBBCN[2] && cantidadTokens < tiersBBCN[3] ) {
            maximoMonto = tiersCARBO[2]; 
        } else if(cantidadTokens > tiersBBCN[3]) {
           maximoMonto = tiersCARBO[3];  
        }
        return maximoMonto;
    }
    function calcularTier() public view returns (uint8) {
        uint256 cantidadTokens = showMyShops(); 
        uint8 numeroTier = 0; 
        if(cantidadTokens >= tiersBBCN[0] && cantidadTokens < tiersBBCN[1]) {
            numeroTier = 1; 
        } else if(cantidadTokens >= tiersBBCN[1] && cantidadTokens < tiersBBCN[2]) {
            numeroTier = 2; 
        } else if(cantidadTokens >= tiersBBCN[2] && cantidadTokens < tiersBBCN[3]) {
            numeroTier = 3; 
        } else if(cantidadTokens > tiersBBCN[3]) {
            numeroTier = 4; 
        }
        return numeroTier;
    }
    function getProxyOwner() external view returns (address) {
        return proxy.owner(); 
    }
}