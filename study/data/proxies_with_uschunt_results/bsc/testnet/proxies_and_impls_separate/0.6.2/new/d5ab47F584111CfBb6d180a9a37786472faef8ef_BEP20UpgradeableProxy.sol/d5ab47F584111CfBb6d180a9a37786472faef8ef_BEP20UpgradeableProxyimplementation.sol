/// @title AlpacaVaultAdapter
///
/// @dev A vault adapter implementation which wraps a yEarn vault.
contract AlpacaVaultAdapterWithIndirection is AlpacaVaultAdapter {
    using FixedPointMath for FixedPointMath.FixedDecimal;
    using SafeERC20Upgradeable for IDetailedERC20;
    using SafeERC20Upgradeable for IAlpacaV2;
    using SafeMathUpgradeable for uint256;
    //address public override governence;
    

    function setAdapter(IAlpacaV2 _vault, address _admin) public  {     
      setInitialValues(_vault, _admin);
    }

    /// @dev Sends vault tokens to the recipient
    ///
    /// This function reverts if the caller is not the admin.
    ///
    /// @param _recipient the account to send the tokens to.
    /// @param _amount    the amount of tokens to send.
    function indirectWithdraw(address _recipient, uint256 _amount) external onlyAdmin {
        vault.transfer(_recipient, _tokensToShares(_amount));
    }
}


