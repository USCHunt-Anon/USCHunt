//import"./library/CraftStruct.sol";

contract Craft is Initializable, ERC721Upgradeable, AccessControlUpgradeable {
    using SafeMathUpgradeable for uint;

    using SafeCastUpgradeable for uint;
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using ERC165CheckerUpgradeable for address;

    //events
    event Summoned(address indexed owner, uint class, uint summoner);
    event Leveled(address indexed owner, uint level, uint summoner);

    event AbilityCreated(address indexed creator, uint summoner, uint32 strength, uint32 dexterity, uint32 constitution, uint32 intelligence, uint32 wisdom, uint32 charisma);
    event AbilityLeveled(address indexed leveler, uint summoner, uint32 strength, uint32 dexterity, uint32 constitution, uint32 intelligence, uint32 wisdom, uint32 charisma);

    //structs

    //variables
    bytes32 public constant GAME_ADMIN_ROLE = keccak256("GAME_ADMIN_ROLE");

    uint constant xpPerDay = 250e18;
    uint constant DAY = 1 days;
    uint constant POINT_BUY = 32;

    uint public nextSummoner;
    mapping(uint => CraftStruct.Summoner) public summoners;
    mapping(uint => CraftStruct.AbilityScore) public abilityScores;
    mapping(uint => uint) public adventurersLog;
    mapping(uint => uint) public levelPointsSpent;
    mapping(uint => bool) public characterCreated;

    function initialize () public initializer {
        __ERC721_init('Craft Manifested', 'CM');
        __AccessControl_init();

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function adventure(uint _summoner) external {
        require(_isApprovedOrOwner(msg.sender, _summoner));
        require(block.timestamp > adventurersLog[_summoner]);
        adventurersLog[_summoner] = block.timestamp + DAY;
        summoners[_summoner].xp += xpPerDay;
    }

    function spendXp(uint _summoner, uint _xp) external {
        require(_isApprovedOrOwner(msg.sender, _summoner));
        summoners[_summoner].xp -= _xp;
    }

    function levelUp(uint _summoner) external {
        require(_isApprovedOrOwner(msg.sender, _summoner));
        CraftStruct.Summoner storage summoner = summoners[_summoner];
        uint _level = summoner.level;
        uint _xpRequired = xpRequired(_level);
        summoner.xp -= _xpRequired;
        summoner.level = _level+1;
        emit Leveled(msg.sender, _level, _summoner);
    }

    function getSummoner(uint _summoner) external view returns (CraftStruct.Summoner memory summoner) {
        return summoners[_summoner];
    }

    function summon(uint _class) external {
        require(balanceOf(msg.sender) < 1, 'exceed');
        require(1 <= _class && _class <= 12, 'invalid class');
        uint _nextSummoner = nextSummoner;
        summoners[_nextSummoner] = CraftStruct.Summoner({
            xp: 0,
            level: 1,
            class: _class
        });
        _safeMint(msg.sender, _nextSummoner);
        emit Summoned(msg.sender, _class, _nextSummoner);
        nextSummoner++;
    }

    function xpRequired(uint currentLevel) public pure returns (uint xpToNextLevel) {
        xpToNextLevel = currentLevel * 1000e18;
        for (uint i = 1; i < currentLevel; i++) {
            xpToNextLevel += currentLevel * 1000e18;
        }
    }

    function tokenURI(uint _summoner) public view override returns (string memory) {
        CraftStruct.Summoner memory summoner = summoners[_summoner];
        string memory output;
        {
        string[5] memory parts;
        parts[0] = '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 350 350"><style>.base { fill: white; font-family: serif; font-size: 14px; }</style><rect width="100%" height="100%" fill="black" /><text x="10" y="20" class="base">';

        parts[1] = string(abi.encodePacked("Summoner", " #", Strings.toString(_summoner), '</text><text x="10" y="40" class="base">'));

        parts[2] = string(abi.encodePacked("Class", " ", CraftStruct.classes(summoner.class), '</text><text x="10" y="60" class="base">'));

        parts[3] = string(abi.encodePacked("Level", " ", Strings.toString(summoner.level), '</text><text x="10" y="80" class="base">'));

        parts[4] = string(abi.encodePacked("Xp", " ", Strings.toString(summoner.xp/1e18), '</text>'));

        output = string(abi.encodePacked(parts[0], parts[1], parts[2], parts[3], parts[4], _encodeAbility(_summoner), '</svg>'));
        }

        string memory json = Base64.encode(bytes(string(abi.encodePacked('{"name": "summoner #', Strings.toString(_summoner), '", "description": "Craft is achieved via an active economy, summoners must level, gain feats, learn spells, to be able to craft gear. This allows for market driven rarity while allowing an ever growing economy. Feats, spells, and summoner gear is ommitted as part of further expansions.", "image": "data:image/svg+xml;base64,', Base64.encode(bytes(output)), '"}'))));
        output = string(abi.encodePacked('data:application/json;base64,', json));

        return output;
    }

    function _encodeAbility(uint256 _summoner) internal view returns (string memory) {
        string memory output;
        {
        string[7] memory parts;
        CraftStruct.AbilityScore memory _attr = abilityScores[_summoner];
        parts[0] = '<text x="10" y="100" class="base">';

        parts[1] = string(abi.encodePacked("Strength", " ", Strings.toString(_attr.strength), '</text><text x="10" y="120" class="base">'));

        parts[2] = string(abi.encodePacked("Dexterity", " ", Strings.toString(_attr.dexterity), '</text><text x="10" y="140" class="base">'));

        parts[3] = string(abi.encodePacked("Constitution", " ", Strings.toString(_attr.constitution), '</text><text x="10" y="160" class="base">'));

        parts[4] = string(abi.encodePacked("Intelligence", " ", Strings.toString(_attr.intelligence),  '</text><text x="10" y="180" class="base">'));

        parts[5] = string(abi.encodePacked("Wisdom", " ", Strings.toString(_attr.wisdom), '</text><text x="10" y="200" class="base">'));

        parts[6] = string(abi.encodePacked("Charisma", " ", Strings.toString(_attr.charisma), '</text>'));

        output = string(abi.encodePacked(parts[0], parts[1], parts[2], parts[3], parts[4], parts[5], parts[6]));
        }
        return output;
    }

    function classes(uint id) public pure returns (string memory description) {
        return CraftStruct.classes(id);
    }

    function getAbilityScore(uint _summoner) external view returns (CraftStruct.AbilityScore memory abilityScore) {
        return abilityScores[_summoner];
    }

    function pointBuy(uint _summoner, uint32 _str, uint32 _dex, uint32 _const, uint32 _int, uint32 _wis, uint32 _cha) external {
        require(_isApprovedOrOwner(msg.sender, _summoner));
        require(!characterCreated[_summoner]);
        require(calculatePointBuy(_str, _dex, _const, _int, _wis, _cha) == POINT_BUY);
        characterCreated[_summoner] = true;

        abilityScores[_summoner] = CraftStruct.AbilityScore({
            strength: _str,
            dexterity: _dex,
            constitution: _const,
            intelligence: _int,
            wisdom: _wis,
            charisma: _cha
        });
        emit AbilityCreated(msg.sender, _summoner, _str, _dex, _const, _int, _wis, _cha);
    }

    function calculatePointBuy(uint _str, uint _dex, uint _const, uint _int, uint _wis, uint _cha) public pure returns (uint) {
        return calcPoint(_str)+calcPoint(_dex)+calcPoint(_const)+calcPoint(_int)+calcPoint(_wis)+calcPoint(_cha);
    }
    function calcPoint(uint score) public pure returns (uint) {
        if (score <= 14) {
            return score - 8;
        } else {
            return ((score - 8)**2)/6;
        }
    }

    function _increaseBase(uint _summoner) internal {
        require(_isApprovedOrOwner(msg.sender, _summoner));
        require(!characterCreated[_summoner]);
        uint _pointsSpent = levelPointsSpent[_summoner];
        require(abilitiesByLevel(summoners[_summoner].level) - _pointsSpent > 0);
        levelPointsSpent[_summoner] = _pointsSpent+1;
    }

    function increaseStrength(uint _summoner) external {
        _increaseBase(_summoner);
        CraftStruct.AbilityScore storage _attr = abilityScores[_summoner];
        _attr.strength = _attr.strength+1;
        emit AbilityLeveled(msg.sender, _summoner, _attr.strength, _attr.dexterity, _attr.constitution, _attr.intelligence, _attr.wisdom, _attr.charisma);
    }

    function increaseDexterity(uint _summoner) external {
        _increaseBase(_summoner);
        CraftStruct.AbilityScore storage _attr = abilityScores[_summoner];
        _attr.dexterity = _attr.dexterity+1;
        emit AbilityLeveled(msg.sender, _summoner, _attr.strength, _attr.dexterity, _attr.constitution, _attr.intelligence, _attr.wisdom, _attr.charisma);
    }

    function increaseConstitution(uint _summoner) external {
        _increaseBase(_summoner);
        CraftStruct.AbilityScore storage _attr = abilityScores[_summoner];
        _attr.constitution = _attr.constitution+1;
        emit AbilityLeveled(msg.sender, _summoner, _attr.strength, _attr.dexterity, _attr.constitution, _attr.intelligence, _attr.wisdom, _attr.charisma);
    }

    function increaseIntelligence(uint _summoner) external {
        _increaseBase(_summoner);
        CraftStruct.AbilityScore storage _attr = abilityScores[_summoner];
        _attr.intelligence = _attr.intelligence+1;
        emit AbilityLeveled(msg.sender, _summoner, _attr.strength, _attr.dexterity, _attr.constitution, _attr.intelligence, _attr.wisdom, _attr.charisma);
    }

    function increaseWisdom(uint _summoner) external {
        _increaseBase(_summoner);
        CraftStruct.AbilityScore storage _attr = abilityScores[_summoner];
        _attr.wisdom = _attr.wisdom+1;
        emit AbilityLeveled(msg.sender, _summoner, _attr.strength, _attr.dexterity, _attr.constitution, _attr.intelligence, _attr.wisdom, _attr.charisma);
    }

    function increaseCharisma(uint _summoner) external {
        _increaseBase(_summoner);
        CraftStruct.AbilityScore storage _attr = abilityScores[_summoner];
        _attr.charisma = _attr.charisma+1;
        emit AbilityLeveled(msg.sender, _summoner, _attr.strength, _attr.dexterity, _attr.constitution, _attr.intelligence, _attr.wisdom, _attr.charisma);
    }

    function abilitiesByLevel(uint currentLevel) public pure returns (uint) {
        return currentLevel / 4;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721Upgradeable, AccessControlUpgradeable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function testadd(uint currentLevel) public pure returns (uint) {
        return currentLevel / 4;
    }
}

