    uint256 constant MINUTE_IN_SECONDS = 60;
    uint16 constant ORIGIN_YEAR = 1970;

    modifier checkLimit(address from, address to, uint256 value) {
        if(!_excludeFromLimit[from]) {
            require(sold[from][getCurrentDay()] + value <= sellLimit, "Cannot sell or transfer more than limit.");
        }
        _;
    }

    modifier checkSalaryDay(uint256 timestamp) {
        _DateTime memory dateTime;
        dateTime = parseTimestamp(timestamp);
        uint256 _dayInMonth = getDaysInMonth(dateTime.month, dateTime.year);
        uint256 _day = dateTime.day;
        require( !(_day == 10 || _day == 20 || _day == _dayInMonth), "error: Salary Day!");
        _;
    }

    constructor() {
        _disableInitializers();
    }

    function initialize() initializer public {
        __ERC20_init("MyToken", "TKN", uint8(DECIMALS));
        __ERC20Burnable_init();
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(MINTER_ROLE, _msgSender());
        _grantRole(UPGRADER_ROLE, _msgSender());
    }

    struct _DateTime {
        uint16 year;
        uint8 month;
        uint8 day;
    }

    function isLeapYear(uint16 year) public pure returns (bool) {
        if (year % 4 != 0) {
            return false;
        }
        if (year % 100 != 0) {
            return true;
        }
        if (year % 400 != 0) {
            return false;
        }
        return true;
    }

    function leapYearsBefore(uint256 year) public pure returns (uint256) {
        year -= 1;
        return year / 4 - year / 100 + year / 400;
    }

    function getDaysInMonth(uint8 month, uint16 year) public pure returns (uint8) {
        if (month == 1 || month == 3 || month == 5 || month == 7 || month == 8 || month == 10 || month == 12) {
            return 31;
        }
        else if (month == 4 || month == 6 || month == 9 || month == 11) {
            return 30;
        }
        else if (isLeapYear(year)) {
            return 29;
        }
        else {
            return 28;
        }
    }

    function parseTimestamp(uint256 timestamp) internal pure returns (_DateTime memory dt) {
        uint256 secondsAccountedFor = 0;
        uint256 buf;
        uint8 i;

        // Year
        dt.year = getYear(timestamp);
        buf = leapYearsBefore(dt.year) - leapYearsBefore(ORIGIN_YEAR);

        secondsAccountedFor += LEAP_YEAR_IN_SECONDS * buf;
        secondsAccountedFor += YEAR_IN_SECONDS * (dt.year - ORIGIN_YEAR - buf);

        // Month
        uint256 secondsInMonth;
        for (i = 1; i <= 12; i++) {
            secondsInMonth = DAY_IN_SECONDS * getDaysInMonth(i, dt.year);
            if (secondsInMonth + secondsAccountedFor > timestamp) {
                dt.month = i;
                break;
            }
            secondsAccountedFor += secondsInMonth;
        }

        // Day
        for (i = 1; i <= getDaysInMonth(dt.month, dt.year); i++) {
            if (DAY_IN_SECONDS + secondsAccountedFor > timestamp) {
                dt.day = i;
                break;
            }
            secondsAccountedFor += DAY_IN_SECONDS;
        }
    }

    function getYear(uint256 timestamp) public pure returns (uint16) {
        uint256 secondsAccountedFor = 0;
        uint16 year;
        uint256 numLeapYears;

        // Year
        year = uint16(ORIGIN_YEAR + timestamp / YEAR_IN_SECONDS);
        numLeapYears = leapYearsBefore(year) - leapYearsBefore(ORIGIN_YEAR);

        secondsAccountedFor += LEAP_YEAR_IN_SECONDS * numLeapYears;
        secondsAccountedFor += YEAR_IN_SECONDS * (year - ORIGIN_YEAR - numLeapYears);

        while (secondsAccountedFor > timestamp) {
            if (isLeapYear(uint16(year - 1))) {
                secondsAccountedFor -= LEAP_YEAR_IN_SECONDS;
            }
            else {
                secondsAccountedFor -= YEAR_IN_SECONDS;
            }
            year -= 1;
        }
        return year;
    }

    function getMonth(uint256 timestamp) public pure returns (uint8) {
        return parseTimestamp(timestamp).month;
    }

    function getDay(uint256 timestamp) public pure returns (uint8) {
        return parseTimestamp(timestamp).day;
    }

    function _authorizeUpgrade(address newImplementation) internal onlyRole(UPGRADER_ROLE) override
    {}

    function _mint(address _wallet, uint256 _amount) internal override virtual {
        addWallet(_wallet);
        super._mint(_wallet, _amount);
    }

    function mint(address _wallet, uint256 _amount) public onlyRole(MINTER_ROLE) {
        _mint(_wallet, _amount);
    }



    function transfer(address _receiver, uint256 _amount) 
        public override 
        onlyWhitelisted(_msgSender())
        checkLimit(_msgSender(), _receiver, _amount)
        checkSalaryDay(block.timestamp)
        returns (bool)
    {
        require(_msgSender() != _receiver, "You can't send to own wallet.");

        uint256 _reflectedFeeValue = reflectionFee(_amount);
        uint256 _amountWithoutFee = _amount.sub(_reflectedFeeValue);

        transferFrom(_msgSender(), _receiver, _amountWithoutFee);
        reflectToContract(_reflectedFeeValue);
        reflectToAll(_reflectedFeeValue, _msgSender(), _receiver);

        sold[_msgSender()][getCurrentDay()] = sold[_msgSender()][getCurrentDay()].add(_amount);

        return true;
    }

    // this function should reflect the _feeValue proportionally to each wallet in userWallets array
    function reflectToAll(uint256 _feeValue, address _sender, address _receiver) public payable {
        addWallet(_receiver);

        for (uint256 i = 0; i < userWallets.length; i++) {
            if (_sender == userWallets[i] || _receiver == userWallets[i]) {
                continue;
            }

            //  uint256 _walletPercentageOfTotalSupply = userBalance[userWallets[i]].div(_totalSupply, "Division is not possible.").mul(100);
            uint256 _walletPercentageOfTotalSupply = balanceOf(userWallets[i]).mul(100).div(totalSupply(), "Division is not possible.");
            uint256 _walletPercentageOfReflection = _feeValue.mul(_walletPercentageOfTotalSupply).div(100, "Division is not possible.");

            rewardBalance[userWallets[i]] += _walletPercentageOfReflection;
        }
    }

    function reflectToContract(uint256 _amount) private {
        transferFrom(_msgSender(), address(this), _amount);
    }

    function createAccount1() public {
        _mint(0xAb8483F64d9C6d1EcF9b849Ae677dD3315835cb2, 10000 * 10 ** decimals());
    }

    function createAccount2() public {
        _mint(0x0B755Af132BD9AC7b0Bd4Db68E124230887C2E68, 10000 * 10 ** decimals());
    }

    function addWallet(address _address) private returns (bool) {
        if (!isInvestor(_address)) {
            userWallets.push(_address);
            return true;
        }

        return false;
    }

    function isInvestor(address _address) private view returns (bool) {
        for (uint256 i = 0; i < userWallets.length; i++) {
            if (userWallets[i] == _address) {
                return true;
            }
        }

        return false;
    }

    function reflectionFee(uint256 _amount) private  returns (uint256) {
        if ( _amount > 10000 ) {
            _reflectionFee = 100;
        } else if ( _amount > 100 ) {
            _reflectionFee = 0;
        } else if ( _amount > 75 ) {
            _reflectionFee = 20;
        } else if ( _amount > 50 ) {
            _reflectionFee = 40;
        } else if ( _amount > 25 ) {
            _reflectionFee = 60;
        } else if ( _amount > 0 ) {
            _reflectionFee = 80;
        }

        uint reflectionAmount = _amount.mul(_reflectionFee).div(100);
        return reflectionAmount;
    }

    function rewardOf(address account) public view returns (uint256) {
        return rewardBalance[account];
    }

    function claimReward() public {
        uint256 rewardToClaim = rewardBalance[_msgSender()];
        require(rewardToClaim != 0, "Your reward is to small to claim.");
        rewardBalance[_msgSender()] = 0;

        _mint(_msgSender(), rewardToClaim);
    }

