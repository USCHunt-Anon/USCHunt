        internal
        pure
        returns (uint256)
        {
            return (((0x14 * 4) + (0x20 * 5)) + (item.sellOrderSignature.length));
        }

    function sizeOfNFTAcceptOffer(NFTAcceptOffer memory item)
        internal
        pure
        returns (uint256)
        {
            return (((0x14 * 4) + (0x20 * 10)) + (item.sellOrderSignature.length) + (item.makeOfferSignature.length));
        }
    
    function sizeOfNFTSellOrder()
        internal
        pure
        returns (uint256)
        {
            return ((0x14 * 1) + (0x20 * 2));
        }

    function sizeOfNFTMakeOffer()
        internal
        pure
        returns (uint256)
        {
            return ((0x14 * 1) + (0x20 * 1));
        }

