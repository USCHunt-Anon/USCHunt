//import{NFTBuyRequest, NFTAcceptOffer, NFTResell, NFTSellOrder, NFTMakeOffer, NFTCancelBatch} from "../common/Structs.sol";

contract SignatureUtils is HashUtils {
    function verifyNFTBuyRequest(
        NFTBuyRequest memory item,
        bytes memory signature,
        address signer
    ) public pure returns (bool) {
        bytes32 messageHash = hashNFTBuyRequest(item);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);
        return recoverSigner(ethSignedMessageHash, signature) == signer;
    }

    function verifyNFTResell(
        NFTResell memory item,
        bytes memory signature,
        address signer
    ) public pure returns (bool) {  
        bytes32 messageHash = hashNFTResell(item);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);
        return recoverSigner(ethSignedMessageHash, signature) == signer;
    }

    function verifyNFTAcceptOffer(
        NFTAcceptOffer memory item,
        bytes memory signature,
        address signer
    ) public pure returns (bool) {
        bytes32 messageHash = hashNFTAcceptOffer(item);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash); 
        return recoverSigner(ethSignedMessageHash, signature) == signer;
    }

    function verifyNFTSellOrder(
        NFTSellOrder memory item,
        bytes memory signature,
        address signer
    ) public pure returns (bool) {
        bytes32 messageHash = hashNFTSellOrder(item);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);
        return recoverSigner(ethSignedMessageHash, signature) == signer;
    }

    function verifyNFTMakeOffer(
        NFTMakeOffer memory item,
        bytes memory signature,
        address signer
    ) public pure returns (bool) {
        bytes32 messageHash = hashNFTMakeOffer(item);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);
        return recoverSigner(ethSignedMessageHash, signature) == signer;
    }

    function verifyNFTCancelBatch(
        NFTCancelBatch memory item,
        bytes memory signature,
        address signer
    ) public pure returns (bool) {
        bytes32 messageHash = hashNFTCancelBatch(item);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);
        return recoverSigner(ethSignedMessageHash, signature) == signer;
    }

    function recoverSigner(bytes32 hash, bytes memory signature)
        public
        pure
        returns (address)
    {
        // (bytes32 r, bytes32 s, uint8 v) = splitSignature(_signature);

        // return ecrecover(_ethSignedMessageHash, v, r, s);
        bytes32 r;
        bytes32 s;
        uint8 v;

        // Check the signature length
        if (signature.length != 65) {
            return (address(0));
        }

        // Divide the signature in r, s and v variables
        // ecrecover takes the signature parameters, and the only way to get them
        // currently is to use assembly.
        // solium-disable-next-line security/no-inline-assembly
        assembly {
            r := mload(add(signature, 0x20))
            s := mload(add(signature, 0x40))
            v := byte(0, mload(add(signature, 0x60)))
        }

        // Version of signature should be 27 or 28, but 0 and 1 are also possible versions
        if (v < 27) {
            v += 27;
        }

        // If the version is correct return the signer address
        if (v != 27 && v != 28) {
            return (address(0));
        } else {
            // solium-disable-next-line arg-overflow
            return ecrecover(hash, v, r, s);
        }
    }
    
}

