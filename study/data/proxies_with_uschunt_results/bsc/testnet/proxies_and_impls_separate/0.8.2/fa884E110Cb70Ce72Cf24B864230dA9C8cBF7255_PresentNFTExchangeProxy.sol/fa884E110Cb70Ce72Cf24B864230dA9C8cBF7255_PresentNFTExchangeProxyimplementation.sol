pragma solidity ^0.8.2;

//import{Collection1155} from "../Collection1155.sol";
//import{Collection721} from "../Collection721.sol"; 
//import"@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
//import"@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
//import"@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
//import"@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

contract Upgradable is ReentrancyGuardUpgradeable {
    address nftAddress;
    address recipientAddress;
    address signatureUtilsAddress;

    address offerHandlerAddress;
    address cancelHandlerAddress;

    uint256 public ratioAdminFee;
    
    mapping(address => bool) adminList;
    mapping(address => bool) acceptedTokenAddress;
    mapping(uint256 => uint256) soldQuantity;
    mapping(bytes => bool) invalidSaleOrder;
    mapping(bytes => bool) invalidOffers;
    mapping(bytes => bool) acceptedOffers;
    mapping(bytes => uint256) soldQuantityBySaleOrder;
    mapping(uint256 => bytes) currentSalesOrderByTokenID;
    mapping(address => uint256) tokensFee;
    mapping(address => uint256) totalFeeAmount;
    mapping(address => bool) signers;
    
    address erc721NftAddress;
    address erc721OfferHandlerAddress;
    address erc721BuyHandlerAddress;
    address erc721OfferHandlerNaviteAddress;
    address buyHandlerNativeAddress;
    address sellHandlerAddress;
    address erc721SellHandlerAddress;
    address sellHandlerNativeAddress;
    address erc721SellHanlderNativeAddress;

    event AcceptOfferEvent(
        uint256 _tokenID,
        uint256 _amount,
        uint256 _pricePerItem,
        uint256 _listingTime,
        uint256 _expirationTime,
        address indexed _receiver,
        address indexed _collectionAddress,
        uint8 _type, // 0: ProcessedOffer, 1: AcceptedOffer, 2: CanceledOffer
        string internalTxID
    );

    event BuyNFTEvent(
        uint256 _tokenID,
        uint256 _amount,
        address indexed _receiver,
        address indexed _collectionAddress,
        uint8 _type, // 0: ProcessingBuyRequest, 1: AcceptedBuyRequest, 2: CanceledBuyRequest
        string internalTxID
    );

    event SellNFTEvent(
        uint256 _tokenID,
        uint256 _amount,
        address indexed _seller,
        address indexed _receiver,
        address indexed _collectionAddress,
        uint8 _type, // 0: ProcessingSellRequest, 1: AcceptedSellRequest, 2: CanceledSellRequest
        string internalTxID
    );

    event CancelEvent(
        address indexed _creator,
        uint256 _type,
        string internalTxID
    );

    event ServiceFeeEvent(
        address indexed _tokenAddress,
        string _currencyId,
        uint256 _feeRate
    );

    event PaymentTokenEvent(
        address indexed _tokenAddress,
        string _currencyId
    );

    modifier onlyAdmins() {
        require(adminList[msg.sender], "Need admin role");
        _;
    }

    function mint721(
        address collection,
        address account,
        uint256 id,
        bytes memory data
    ) internal {
        Collection721(collection).mint(account, id, data);
    }

    function mint1155(
        address collection,
        address account,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) internal {
        Collection1155(collection).mint(account, id, amount, data);
    }

    function transferNFT721(
        address collection,
        address from,
        address to,
        uint256 id,
        bytes memory data
    ) internal {
        Collection721(collection).safeTransferFrom(from, to, id, data);
    }

    function transferNFT1155(
        address collection,
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) internal {
        Collection1155(collection).safeTransferFrom(from, to, id, amount, data);
    }

    function setAdminList(address _address, bool value) public {
        adminList[_address] = value;
    }

    function isAdmin(address _address) public view returns (bool) {
        return adminList[_address];
    }
}


