//import"@openzeppelin/contracts/security/ReentrancyGuard.sol";
//import"contracts/relayer/BasicMetaTransaction.sol";
//import"./upgradeability/CustomOwnable.sol";
//import"./interfaces/brainchainEvents.sol";
//import"./EQ8Tokens.sol";
//import"./BadgeToken.sol";
//import"./NFT.sol";
//import"@openzeppelin/contracts/access/AccessControl.sol";

contract Admin is ReentrancyGuard, brainchainEvents, AccessControl{

    uint16 public BadgeReward;
    uint8 public newUserReward;
    uint8 public newQuestion;
    uint8 public surveyReward;
    uint16 public questionUsed;
    uint16 public totalBadges;
    uint public participateReward;
    address public root;
    uint64 public counterId;//SurveyId
    uint public redeem;
    uint public PlatformFee;
    uint public RoyaltyFee;
    uint public pollWin;
    bool internal isInitialized;

    address public equate;
    address public ExpNTT;
    address public Nft;
    address public userContract;
    address public questionContract;
    address public pollContract;
    address public surveyContract;
    address public operator;

//    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    mapping(uint => uint64) public rewardPercentages;  // uint => percentage for kyc, badge, rarity
    mapping(address => uint) public redeemedNTT;


    function initialize(address _root, address _equate, address _badgeToken, address _nft) public {
        require(!isInitialized,"initialized");
        isInitialized = true;
         root = _root;
        _setupRole(DEFAULT_ADMIN_ROLE, root);
        // _setupRole(MINTER_ROLE, root);
       
        counterId = 1;
        equate = _equate;
        ExpNTT = _badgeToken;
        Nft = _nft;

        participateReward = 1;
        pollWin = 9;
        BadgeReward = 10;
        newUserReward = 10;
        newQuestion = 50;
        surveyReward = 20;
        questionUsed = 100;
        redeem = 100 * 10 **18;
        PlatformFee = 10;
        RoyaltyFee = 5;// change

    }


    function setRedeem(uint _amount) external onlyRole(DEFAULT_ADMIN_ROLE){
        redeem = _amount;
    }

    function setBadgeReward(uint val) external onlyRole(DEFAULT_ADMIN_ROLE){
        BadgeReward = uint16(val);
    }

    function setNewUserReward(uint val) external onlyRole(DEFAULT_ADMIN_ROLE){
        newUserReward = uint8(val);
    }

    function setNewQuestion(uint8 _val) external onlyRole(DEFAULT_ADMIN_ROLE){
        newQuestion = _val;
    }

    function setSurveyReward(uint8 _val) external onlyRole(DEFAULT_ADMIN_ROLE){
        surveyReward = _val;
    }
    
    function setQuestionUsed(uint16 _val) external onlyRole(DEFAULT_ADMIN_ROLE){
        questionUsed = _val;
    }

    function setPlatformFee(uint8 _val) external onlyRole(DEFAULT_ADMIN_ROLE){
        PlatformFee = _val;
    }

    function setRoyaltyFee(uint8 _val) external onlyRole(DEFAULT_ADMIN_ROLE){
        RoyaltyFee = _val;
    }

    function setRewardPercentages(uint64 _kyc, uint64 _badges, uint64 _rarity) external onlyRole(DEFAULT_ADMIN_ROLE){
        require(_kyc + (_badges + (_rarity)) == 100,"not equal to 100");
        rewardPercentages[1] = _kyc;
        rewardPercentages[2] = _badges;
        rewardPercentages[3] = _rarity;
    }

    function changeAddress(uint id, address _changeAddress) external {
        require(id > 0 && id < 4, " id should not be less than 1 or more than 3");
        if(id == 1){
            equate = _changeAddress;
        }else if(id == 2){
            ExpNTT = _changeAddress;
        }else if(id == 3){
            Nft = _changeAddress;
        }
    }

    function setBadgesAmount(uint val) external onlyRole(DEFAULT_ADMIN_ROLE){
        totalBadges = uint16(val);
    }

    function setPollParticipate(uint value) external onlyRole(DEFAULT_ADMIN_ROLE){
        participateReward = value;
    }

    function setPollWin(uint value) external onlyRole(DEFAULT_ADMIN_ROLE){
        pollWin = value;
    }

    function increaseCounterID() external {
        counterId++;
    }

    function setUserContract(address _userContract) external onlyRole(DEFAULT_ADMIN_ROLE){
        userContract = _userContract;
        EQ8token(equate).addMinter(_userContract); 
        BadgeToken(ExpNTT).addMinter(_userContract);
        NFT(Nft).addMinter(_userContract);
    }

    function setQuestionContract(address _questionContract) external onlyRole(DEFAULT_ADMIN_ROLE){
        questionContract = _questionContract;
        EQ8token(equate).addMinter(_questionContract); 
        BadgeToken(ExpNTT).addMinter(_questionContract);
        NFT(Nft).addMinter(_questionContract);
    }

    function setPollContract(address _pollContract) external onlyRole(DEFAULT_ADMIN_ROLE){
        pollContract = _pollContract;
        EQ8token(equate).addMinter(_pollContract); 
        BadgeToken(ExpNTT).addMinter(_pollContract);
        NFT(Nft).addMinter(_pollContract);
    } 

    function setSurveyContract(address _surveyContract) external onlyRole(DEFAULT_ADMIN_ROLE){
        surveyContract = _surveyContract;
        EQ8token(equate).addMinter(_surveyContract); 
        BadgeToken(ExpNTT).addMinter(_surveyContract);
        NFT(Nft).addMinter(_surveyContract);
    } 

    function redeemNTT() external { 
        require(BadgeToken(ExpNTT).balanceOf(_msgSender()) - redeemedNTT[_msgSender()] >= redeem,"amount greater than redeem");  
        uint amount = (BadgeToken(ExpNTT).balanceOf(_msgSender()) - redeemedNTT[_msgSender()]) / redeem; 
        EQ8token(equate)._mint(_msgSender(),amount * 10 ** 18); 
        redeemedNTT[_msgSender()] += amount * redeem; 
    } 

    function setOperator(address _operator) external onlyRole(DEFAULT_ADMIN_ROLE){
        operator = _operator;  
    }

    

}

