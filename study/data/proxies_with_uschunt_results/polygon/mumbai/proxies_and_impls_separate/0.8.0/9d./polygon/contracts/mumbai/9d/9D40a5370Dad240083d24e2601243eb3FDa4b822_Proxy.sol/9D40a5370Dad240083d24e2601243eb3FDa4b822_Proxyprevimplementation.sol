    function pull(address owner_, uint256[] calldata ids) external {
        require (msg.sender == castle, "not castle");
        for (uint256 index = 0; index < ids.length; index++) {
			uint256 tokenId = ids[index];
			require(tokenId <= 10_000, "not genesis");
			(uint256 job, uint256 task, ) = space.getLife(tokenId);
			if (job > 0 || task > 0) {
				space.exit(tokenId);
			}
            _transfer(owner_, msg.sender, ids[index]);
        }
        CastleLike(msg.sender).pullCallback(owner_, ids);
    }

    function adjustApe(uint256 id, string memory name, uint256 trait_, uint256 info) external {
        require(msg.sender == admin || msg.sender == castle, "not authorized");
		names[id] = name;
		tokenTraits[id] = trait_;
		setInfo(id, info);
    }

	function mintAbucks(address to, uint256 amount) external virtual {
		require (msg.sender == castle, "not castle");
		bucks.mint(to, amount);
