    function setTowerToken(address _towerToken) external {
        require(accessControls.hasAdminRole(_msgSender()), "NFTStaking: Unauthorized.");
        require(_towerToken != address(0));

        towerToken = ITOWER(_towerToken);
    }
    
    function setCDHNFTContractAddress(address _nftContract) external {
        require(accessControls.hasAdminRole(_msgSender()), "NFTStaking: Unauthorized.");
        require(_nftContract != address(0));

