    function stake(uint256 _tokenId) override external {
        internalStake(_msgSender(), _tokenId);
    }

    function stakeAll() override external {
        internalStakeAll(_msgSender());
    }

    function stakeTokens(uint256[] memory tokenIds) override external {
        address player = _msgSender();
