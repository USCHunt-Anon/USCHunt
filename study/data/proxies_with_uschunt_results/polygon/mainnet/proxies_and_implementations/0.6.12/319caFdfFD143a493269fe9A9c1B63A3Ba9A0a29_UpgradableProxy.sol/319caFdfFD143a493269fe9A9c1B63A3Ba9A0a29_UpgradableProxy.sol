contract TinyHeroFusion is IERC721Receiver, OwnableUpgradeable, ReentrancyGuard {
    using SafeERC20 for IERC20;

    mapping(address => bool) private _admin;

    address private nftTiny_;
    address private nftSuperTiny_;
    address private tiny_;
    address private tinyStats_;
    address private superTinyFactory_;


    uint256 public feeFusion;
    uint256 public feeRedeem;
    uint256 public totalTinyBurn;
    uint256 public currentSeed;

    uint256 nonce;

    uint256 public burnRate;
    uint256 public reserveRate;
    address public reserveFund;

    mapping(uint256 => uint256[]) public tinyTokenIDs;

    address public tinyStats = address(0xEB46eE67BB4a3edBaB49eEA8a2e492709f355D9b);

    address public deadWallet = 0x000000000000000000000000000000000000dEaD;

    /* ========== EVENTS ========== */

    event OnERC721Received(address operator, address from, uint256 tokenId, bytes data);
    event AdminUpdate(address indexed account, bool isAdmin);
    event BurnFee(uint256 indexed tokenId, uint256 fee);
    event ReserveFee(uint256 indexed tokenId, uint256 fee);
    event Fusion(uint256 indexed tokenId);
    event Redeem(uint256 indexed tokenId);


    /* ========== Modifiers =============== */

    modifier onlyAdmin() {
        require(_admin[msg.sender] || owner() == msg.sender, "!admin");
        _;
    }

    /* ========== GOVERNANCE ========== */

    function initialize(address _nftTiny, address _nftSuperTiny, address _tiny, address _tinyStats, address _superTinyFactory) external initializer {
        require(_nftTiny != address(0), "zero");
        __Ownable_init();
        nftTiny_ = _nftTiny;
        nftSuperTiny_ = _nftSuperTiny;
        tiny_ = _tiny;
        tinyStats_ = _tinyStats;
        superTinyFactory_ = _superTinyFactory;
        burnRate = 2000; // 20%
        reserveRate = 0; // 0%
        reserveFund = msg.sender; // temporarily
    }

    function setAdmin(address _account, bool _isAdmin) external onlyOwner {
        require(_account != address(0), "zero");
        emit AdminUpdate(_account, _isAdmin);
        _admin[_account] = _isAdmin;
    }

    function setFeeRates(uint256 _burnRate, uint256 _reserveRate, uint256 _feeFusion, uint256 _feeRedeem) external onlyOwner {
        burnRate = _burnRate;
        reserveRate = _reserveRate;
        feeFusion = _feeFusion;
        feeRedeem = _feeRedeem;
    }

    function setTinyInfo(address _tinyStats, address _superTinyFactory) external onlyOwner {
        require(_tinyStats != address(0) && _superTinyFactory != address(0), "zero");
        tinyStats = _tinyStats;
        _superTinyFactory = _superTinyFactory;
    }

    function setReserveFund(address _reserveFund) external onlyOwner {
        require(_reserveFund != address(0), "zero");
        reserveFund = _reserveFund;
    }

    /* ========== VIEW FUNCTIONS ========== */

    function isAdmin(address _account) external view returns (bool) {
        return _admin[_account];
    }

    function nft() external view returns (address) {
        return nftTiny_;
    }

    function tiny() external view returns (address) {
        return tiny_;
    }

 /* ========== MUTATIVE FUNCTIONS ========== */


    function onERC721Received(address operator, address from, uint256 tokenId, bytes calldata data) external override returns (bytes4) {
        if (msg.sender == nftTiny_ || msg.sender == nftSuperTiny_) {
            emit OnERC721Received(operator, from, tokenId, data);
            return this.onERC721Received.selector;
        } else revert();
    }

    function rad3Num(uint256 _t1, uint256 _t2, uint256 _t3, uint256 _nonce) private returns (uint256 num) {
        currentSeed = _genNextSeed(_t1 ^ _t2 ^ _t3 ^ _nonce);
        uint256 maxRanger = 4;
        uint256 rad =  uint256(keccak256(abi.encodePacked(currentSeed, maxRanger))) % 3;
        num = _t3;
        if (rad == 0) num = _t1; else
        if (rad == 1) num = _t2;
    }

    function getSubIds(uint256 _tokenId) public view returns (uint256[] memory _subIds) {
        _subIds = new uint256[](7);
        uint256 _tmpTokenId = _tokenId;
        for (uint i = 0; i < 7; i++) {
            _subIds[i] = _tmpTokenId % (2 ** 8);
            _tmpTokenId = _tmpTokenId >> 8;
        }
    }

    function generateID(uint256 _body, uint256 _eye, uint256 _mouth, uint256 _hair, uint256 _horn, uint256 _type, uint256 _rarity) private returns (uint256 _superTinyID) {
        _superTinyID = _type << 48;
        _superTinyID = _superTinyID | _rarity << 40;
        _superTinyID = _superTinyID | _hair << 32;
        _superTinyID = _superTinyID | _eye << 24;
        _superTinyID = _superTinyID | _mouth << 16;
        _superTinyID = _superTinyID | _body << 8;
        _superTinyID = _superTinyID | _horn << 0; //8bit
    }

    function fusionToken(uint256[] memory parents, uint256 _rarity) private returns (uint256 _fusedId) {
        ISuperTinyNFT _nftSuperTiny = ISuperTinyNFT(nftSuperTiny_);
        uint256[] memory fSubIds = getSubIds(parents[0]);
        uint256[] memory sSubIds = getSubIds(parents[1]);
        uint256[] memory tSubIds = getSubIds(parents[2]);
        uint256[] memory _subIds = new uint256[](7);
        for (uint256 _attempt = 1; _attempt <= 3; _attempt++) {
            nonce = nonce + 1;
            for (uint i = 0; i < 7; i++) {
                _subIds[i] = rad3Num(fSubIds[i], sSubIds[i], tSubIds[i], nonce);
            }
            uint256 _type;
            if (_subIds[1] == fSubIds[1]) {
                _type = fSubIds[6];
            } else if (_subIds[1] ==  tSubIds[1]){
                _type = tSubIds[6];
            } else {
                _type = sSubIds[6];
            }
            _fusedId = generateID(_subIds[1], _subIds[3], _subIds[2], _subIds[4], _subIds[0], _type, _rarity);
            if (!_nftSuperTiny.exists(_fusedId)) {
                console.log(nonce, _fusedId);
                return _fusedId;
            }
        }
        revert("Fail to mint new token (duplicated)");
    }
    function _genNextSeed(uint256 _newHash) internal view returns (uint256) {
        return currentSeed ^ _newHash ^ uint256(blockhash(block.number - 1));
    }

    function getLuckyNumber(uint256 _randomNumber) private pure returns (uint256) {
        uint256 maxRanger = 40;
        return uint256(keccak256(abi.encodePacked(_randomNumber, maxRanger))) % 40 + 80;
    }

    function getTinyScore(uint256 _tokenId) internal view returns (uint256) {
        (ITinyHeroStats.TinyInfo memory info, ITinyHeroStats.TinyAttribute memory attribute) =  ITinyHeroStats(tinyStats_).details(_tokenId);
        uint256 sum = attribute.Agility + attribute.Intelligence + attribute.Strength + attribute.Atk;
        if (info.Type == ITinyHeroStats.TinyType.STRENGTH)  {
            sum -= attribute.Strength;
        } else if (info.Type == ITinyHeroStats.TinyType.AGILITY) {
            sum -= attribute.Agility;
        } else {
            sum -= attribute.Intelligence;
        }
        return sum;
    }

    function getFusionScore(uint256[] memory parents, uint256 _luckyNumber) internal view returns (uint256) {
        uint256 _fusionScore = getTinyScore(parents[0]) + getTinyScore(parents[1]) + getTinyScore(parents[2]);
        return (_fusionScore * _luckyNumber / 100);
    }

    function fusion(uint256[] memory parents , uint256 _userRandomHash) external nonReentrant {
        require(parents.length == 3, "invalid parents id");
        BRC721 _nftTiny = BRC721(nftTiny_);
        BRC721 _nftSuperTiny = BRC721(nftSuperTiny_);
        uint256 length = parents.length;
        uint256 cntTypeStrength = 0;
        uint256 cntTypeAgility = 0;
        uint256 cntTypeIntelligence = 0;
        uint256 fusionScore = 0;
        currentSeed = _genNextSeed(_userRandomHash);
        for (uint256 i = 0; i < length; i++) {
            require(_nftTiny.ownerOf(parents[i]) == msg.sender, "!own");
            _nftTiny.safeTransferFrom(msg.sender, address(this), parents[i]);
            (ITinyHeroStats.TinyInfo memory info, ITinyHeroStats.TinyAttribute memory att) = ITinyHeroStats(tinyStats_).details(parents[i]);
            if (info.Type == ITinyHeroStats.TinyType.INTELLIGENCE) cntTypeIntelligence++;
            if (info.Type == ITinyHeroStats.TinyType.AGILITY) cntTypeAgility++;
            if (info.Type == ITinyHeroStats.TinyType.STRENGTH) cntTypeStrength++;
            fusionScore = att.Agility + att.Strength + att.Intelligence +  att.Atk;
        }
        require(cntTypeStrength == 1 && cntTypeAgility == 1 && cntTypeIntelligence == 1, "not enough tiny type");
        uint256 _luckyNumber = getLuckyNumber(currentSeed); // 80 - 120
        uint256 _fusionScore = getFusionScore(parents, _luckyNumber);
        ISuperTinyStats.Class _classSuperTiny = ISuperTinyStats.Class.COMMON ; // Default COMMON
        uint16 _point = 0;
        uint16 _turn = 3;
        if (_fusionScore > 312 && _fusionScore <= 408) {
            // UNCOMMON
            _classSuperTiny = ISuperTinyStats.Class.UNCOMMON;
            _turn = 6;
        }
        if (_fusionScore > 408 && _fusionScore <= 504) {
            // RARE
            _classSuperTiny = ISuperTinyStats.Class.RARE;
            _turn = 9;
        }
        if (_fusionScore > 504 && _fusionScore <= 600) {
            // EPIC
            _classSuperTiny = ISuperTinyStats.Class.EPIC;
            _turn = 12;
        }
        if (_fusionScore > 600 && _fusionScore <= 696) {
            // LEGENDARY
            _classSuperTiny = ISuperTinyStats.Class.LEGENDARY;
            _turn = 15;
        }
        if (_fusionScore > 696 && _fusionScore <= 792) {
            // MYTHIC
            _classSuperTiny = ISuperTinyStats.Class.MYTHIC;
            _turn = 18;
        }
        uint256 _fusedTokenId = fusionToken(parents, _turn / 3 + 10);

        if (_nftSuperTiny.ownerOf(_fusedTokenId) != address(this)) {
            tinyTokenIDs[_fusedTokenId].push(parents[0]);
            tinyTokenIDs[_fusedTokenId].push(parents[1]);
            tinyTokenIDs[_fusedTokenId].push(parents[2]);
        }

        ISuperTinyStats.SuperTinyInfo memory info = ISuperTinyStats.SuperTinyInfo(
            _classSuperTiny,
            _point,
            _turn
        );

        ISuperTinyFactory _superTinyFactory =  ISuperTinyFactory(superTinyFactory_);
        if (_nftSuperTiny.ownerOf(_fusedTokenId) == address(this)) {
            _nftSuperTiny.safeTransferFrom(address(this), msg.sender, _fusedTokenId);
        } else {
            _superTinyFactory.mintToken(msg.sender, _fusedTokenId, info);
        }
        IERC20(tiny_).safeTransferFrom(msg.sender, address(this), feeFusion);
        uint restAmount = feeFusion;
        if (burnRate > 0) {
            uint256 _burnFee = feeFusion * burnRate / 10000;
            restAmount -= _burnFee;
            IERC20(tiny_).safeTransfer(deadWallet, _burnFee);
            totalTinyBurn += _burnFee;
            emit BurnFee(_fusedTokenId, _burnFee);
        }

        if (restAmount > 0) {
            IERC20(tiny_).safeTransfer(reserveFund, restAmount);
            emit ReserveFee(_fusedTokenId, restAmount);
        }
        emit Fusion(_fusedTokenId);
    }

    function redeem(uint256 _tokenId) external nonReentrant {
        ISuperTinyFactory _superTinyFactory =  ISuperTinyFactory(superTinyFactory_);
        BRC721 _nftSuperTiny = BRC721(nftSuperTiny_);
        BRC721 _nftTiny = BRC721(nftTiny_);
        require(_nftSuperTiny.ownerOf(_tokenId) == msg.sender, "!own");
        _nftSuperTiny.safeTransferFrom(msg.sender, address(this), _tokenId);
        for (uint256 i = 0; i < 3; i++) {
            _nftTiny.safeTransferFrom(address(this), msg.sender, tinyTokenIDs[_tokenId][i]);
        }
        IERC20(tiny_).safeTransferFrom(msg.sender, address(this), feeRedeem);
        uint restAmount = feeRedeem;
        if (burnRate > 0) {
            uint256 _burnFee = feeRedeem * burnRate / 10000;
            restAmount -= _burnFee;
            IERC20(tiny_).safeTransfer(deadWallet, _burnFee);
            totalTinyBurn += _burnFee;
            emit BurnFee(_tokenId, _burnFee);
        }

        if (restAmount > 0) {
            IERC20(tiny_).safeTransfer(reserveFund, restAmount);
            emit ReserveFee(_tokenId, restAmount);
        }
        emit Redeem(_tokenId);
    }

    /* ========== EMERGENCY ========== */

    function rescueStuckErc20(address _token) external onlyOwner {
        IERC20(_token).transfer(owner(), IERC20(_token).balanceOf(address(this)));
    }

    function rescueStuckErc721(uint256 _tokenId) external onlyOwner {
        BRC721 _nftTiny = BRC721(nftTiny_);
        _nftTiny.safeTransferFrom(address(this), msg.sender, _tokenId);
    }
}