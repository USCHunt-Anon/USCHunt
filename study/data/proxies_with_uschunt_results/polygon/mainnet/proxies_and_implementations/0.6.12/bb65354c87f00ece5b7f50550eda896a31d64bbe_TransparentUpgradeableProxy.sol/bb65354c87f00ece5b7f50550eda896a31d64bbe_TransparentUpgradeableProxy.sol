pragma solidity ^0.8.3;

//import"./Base64.sol";
//import"./IPolygod.sol";

//import"@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155SupplyUpgradeable.sol";
//import"@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
//import"@openzeppelin/contracts/utils/Counters.sol";

contract Polygod is ERC1155SupplyUpgradeable, OwnableUpgradeable, IPolygod {
    using Counters for Counters.Counter;

    /// @dev define game object types
    enum Type {
        WARRIOR,
        ARCHER,
        RIDER,
        BERSERKER,
        AIR,
        GOD
    }

    string public name;
    string public symbol;

    Counters.Counter currentTokenId;

    mapping(uint256 => mapping(uint256 => BasicAttr)) public basicAttrs;
    mapping(uint256 => Attr) public attrs;
    mapping(uint256 => string) public typeString;
    mapping(uint256 => uint256) private hps;
    mapping(uint256 => uint256) private lastTimeStamps;
    mapping(address => bool) public admins;
    uint8[6] public _requirements;

    function initialize() public initializer {
        __ERC1155Supply_init();
        OwnableUpgradeable.__Ownable_init();
        name = "Polygod";
        symbol = "Polygod";

        updateType("Warrior", 0);
        updateType("ARCHER", 1);
        updateType("RIDER", 2);
        updateType("BERSERKER", 3);
        updateType("AIR", 4);
        updateType("God", 5);
        _requirements = [5, 4, 3, 2, 2, 2];
    }

    function _mintWarrior(
        address to,
        uint8 _typeId,
        uint8 _level
    ) internal onlyAdmins {
        uint256 tokenId = currentTokenId.current();
        currentTokenId.increment();
        _mint(to, tokenId, 1, "");
        BasicAttr memory bAttr = basicAttrs[_typeId][_level];
        attrs[tokenId] = Attr({typeId: _typeId, level: _level});
        hps[tokenId] = bAttr.maxHP;
        lastTimeStamps[tokenId] = block.timestamp;
    }

    function addAdminRole(address admin) public onlyOwner {
        admins[admin] = true;
    }

    function revokeAdminRole(address admin) public onlyAdmins {
        admins[admin] = false;
    }

    function adminRole(address admin) public view returns (bool) {
        return admins[admin];
    }

    /// fallback function (if exists)

    /// external
    function setBasicAttrs(BasicAttr[] memory _basicAttrs) external onlyAdmins {
        uint256 len = _basicAttrs.length;
        for (uint256 i = 0; i < len; i++) {
            BasicAttr memory _bAttr = _basicAttrs[i];
            basicAttrs[_bAttr.typeId][_bAttr.level] = _bAttr;
        }
    }

    function batchUpdate(uint256[] memory _ids, uint256[] memory _hps)
        external
        override
        onlyAdmins
    {
        uint256 len = _ids.length;
        for (uint256 i = 0; i < len; i++) {
            uint256 id = _ids[i];
            hps[id] = _hps[i];
            lastTimeStamps[id] = block.timestamp;
        }
    }

    function batchUpdateHpHalf(uint256[] memory _ids)
        external
        override
        onlyAdmins
    {
        uint256 len = _ids.length;
        for (uint256 i = 0; i < len; i++) {
            uint256 id = _ids[i];
            Attr memory attr = attrs[id];
            BasicAttr memory battr = basicAttrs[attr.typeId][attr.level];
            uint256 maxHp = battr.maxHP;
            hps[id] = maxHp / 2;
            lastTimeStamps[id] = block.timestamp;
        }
    }

    function timeStamp() internal view returns (uint256) {
        return block.timestamp;
    }

    function batchMintCount(Attr memory _attr, uint256 count)
        external
        override
        onlyOwner
    {
        for (uint256 i = 0; i < count; i++) {
            _mintWarrior(_msgSender(), _attr.typeId, _attr.level);
        }
    }

    /// @dev should defifne access modifier
    function batchMintTo(
        Attr memory _attr,
        uint256 count,
        address to
    ) public override {
        for (uint256 i = 0; i < count; i++) {
            _mintWarrior(to, _attr.typeId, _attr.level);
        }
    }

    function updateType(string memory _name, uint256 id) public onlyAdmins {
        typeString[id] = _name;
    }

    /// @dev require some validation check
    function batchGodMint(BasicAttr[] memory _attrs) external onlyOwner {
        uint256 len = _attrs.length;
        for (uint256 i = 0; i < len; i++) {
            BasicAttr memory _bAttr = _attrs[i];
            basicAttrs[_bAttr.typeId][_bAttr.level] = _bAttr;
            _mintWarrior(_msgSender(), _bAttr.typeId, _bAttr.level);
        }
    }

    /// public
    function supply() public view override returns (uint256) {
        return currentTokenId.current();
    }

 
    function uri(uint256 tokenId) public view override returns (string memory) {
        Attr memory attr = attrs[tokenId];
        BasicAttr memory bAttr = basicAttrs[attr.typeId][attr.level];
        uint256 hp = getHp(tokenId);

        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        '{"name": "',bAttr.name,'#',Base64.uint2str(tokenId),'",',
                        '"image": "',
                        bAttr.artUri,
                        '",',
                        '"attributes": [{"trait_type": "Type", "value": "',
                        typeString[bAttr.typeId],
                        '"},',
                        '{"trait_type": "Damage", "value": "',
                        Base64.uint2str(bAttr.damage),
                        '"},',
                        '{"trait_type": "Armour", "value": "',
                        Base64.uint2str(bAttr.armour),
                        '"},',
                        '{"trait_type": "Stamina", "value": "',
                        Base64.uint2str(bAttr.stamina),
                        '"},',
                        '{"trait_type": "Agility", "value": "',
                        Base64.uint2str(bAttr.agility),
                        '"},',
                        '{"trait_type": "MaxHP", "value": "',
                        Base64.uint2str(bAttr.maxHP),
                        '"},',
                        '{"trait_type": "HP", "value": "',
                        Base64.uint2str(hp),
                        '"},',
                        '{"trait_type": "Level", "value": "',
                        Base64.uint2str(attr.level),
                        '"}',
                        "]}"
                    )
                )
            )
        );
        return string(abi.encodePacked("data:application/json;base64,", json));
    }

    /// internal
    function recover(uint256 id) public override onlyAdmins {
        Attr memory attr = attrs[id];
        BasicAttr memory battr = basicAttrs[attr.typeId][attr.level];
        uint256 maxHp = battr.maxHP;
        hps[id] = maxHp;
    }

    function getHp(uint256 id) internal view returns (uint256) {
        uint256 lastHp = hps[id];
        uint256 lastTimeStamp = lastTimeStamps[id];
        Attr memory attr = attrs[id];
        BasicAttr memory battr = basicAttrs[attr.typeId][attr.level];
        uint256 maxHp = battr.maxHP;
        uint256 recoveryTime = battr.recoveryTime;
        uint256 timePassed = block.timestamp - lastTimeStamp;
        uint256 hpIncrease = (maxHp * timePassed) / recoveryTime;
        uint256 hp = lastHp + hpIncrease;
        if (hp > maxHp) hp = maxHp;
        return hp;
    }

    function setRequirements(uint8[] memory _reqs) external onlyAdmins {
        for (uint8 i = 0; i < _reqs.length; i++) _requirements[i] = _reqs[i];
    }

    function getRequirements() public view override returns (uint8[] memory) {
        uint8[] memory requirements = new uint8[](_requirements.length);
        for (uint8 i = 0; i < _requirements.length; i++)
            requirements[i] = _requirements[i];

        return requirements;
    }

    function canPlay(uint256[] memory _ids)
        external
        view
        override
        returns (bool, uint256)
    {
        uint256 len = _ids.length;
        uint256 count = 0;
        uint8[6] memory requirements = _requirements;

        for (uint256 i = 0; i < len; i++) {
            uint256 id = _ids[i];
            require(
                requirements[attrs[id].typeId] > 0,
                string(abi.encodePacked(typeString[attrs[id].typeId], " issue"))
            );
            requirements[attrs[id].typeId]--;
            count += attrs[id].level;
        }

        return (
            (requirements[uint8(Type.WARRIOR)] == 0 &&
                requirements[uint8(Type.ARCHER)] == 0 &&
                requirements[uint8(Type.RIDER)] == 0 &&
                requirements[uint8(Type.BERSERKER)] == 0 &&
                requirements[uint8(Type.AIR)] == 0 &&
                (requirements[uint8(Type.GOD)] >= 0 &&
                    requirements[uint8(Type.GOD)] <= 2)),
            count
        );
    }

    function _onlyAdmins() internal view {
        require(
            admins[msg.sender] || owner() == msg.sender,
            "You don't have permission"
        );
    }

    modifier onlyAdmins() {
        _onlyAdmins();
        _;
    }
}


}
