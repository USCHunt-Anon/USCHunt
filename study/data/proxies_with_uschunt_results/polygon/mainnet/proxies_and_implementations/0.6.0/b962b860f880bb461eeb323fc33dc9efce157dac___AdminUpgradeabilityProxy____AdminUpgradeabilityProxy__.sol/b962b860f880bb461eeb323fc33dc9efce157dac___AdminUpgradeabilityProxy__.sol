contract GuardClaim is Governable,ReentrancyGuardUpgradeSafe{
    
//    using Address for address payable;
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    mapping(address => uint) public claimingList;
    mapping(address => uint) public claimedList;
    IERC20 public  claimToken;
    address public operator;
    address public mine;
    uint256 public totalClaiming;

    
    function __claim_init(
        address _governor,address _claimToken,address _mine
    ) public initializer {
	    __Governable_init_unchained(_governor);
        __ReentrancyGuard_init_unchained();
        __claim_init_unchained(_claimToken,_mine);
    }

    function __claim_init_unchained(
        address _claimToken,address _mine
    ) public governance {
        claimToken = IERC20(_claimToken);
        mine = _mine;
    }
    
    function setOperate(address _operator) external governance {
        operator = _operator;
    }
    
    
 

    function addClaimingList(address payable[] calldata dsts, uint value) external  {
        require((isGov()||msg.sender==operator),"No permission!");
        uint256 len = dsts.length;
        for(uint i=0; i<len; i++)
            claimingList[dsts[i]] = claimingList[dsts[i]].add(value);
        totalClaiming=totalClaiming.add(value.mul(len));
    }
    
    
    
    function addClaimingList(address payable[] calldata dsts, uint[] calldata values) external   {
        require((isGov()||msg.sender==operator),"No permission!");
        uint256 _sum = 0;
        for(uint i=0; i<dsts.length; i++){
            claimingList[dsts[i]] = claimingList[dsts[i]].add(values[i]);
            _sum = _sum.add(values[i]);
        }
        totalClaiming=totalClaiming.add(_sum);
    }
    

    function claim() external payable {
        require(claimingList[msg.sender]>0,"No amount");
        uint amount = claimingList[msg.sender];
        claimingList[msg.sender] = 0;
        claimToken.transferFrom(mine,msg.sender,amount);
        claimedList[msg.sender] = claimedList[msg.sender].add(amount);
        emit Claim(msg.sender,amount,claimedList[msg.sender]);
        
        
    }
    event Claim(address indexed to, uint256 value, uint256 totalClaimed);

    function claimAll() external payable governance {
            uint amount = claimToken.balanceOf(address(this));
            claimingList[msg.sender] = 0;
            claimToken.transferFrom(mine,msg.sender, amount);
            claimedList[msg.sender] = claimedList[msg.sender].add(amount);
            emit Claim(msg.sender,amount,claimedList[msg.sender]);
    }

}