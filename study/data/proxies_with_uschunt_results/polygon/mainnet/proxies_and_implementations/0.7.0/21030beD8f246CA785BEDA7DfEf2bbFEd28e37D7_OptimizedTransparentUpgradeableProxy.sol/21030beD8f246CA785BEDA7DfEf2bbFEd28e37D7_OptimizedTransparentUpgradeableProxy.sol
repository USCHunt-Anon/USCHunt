		view
		override
		returns (uint256[] memory)
	{
		require(_exists(tokenId), 'Token does not exist');

		return _equipments[tokenId];
	}

	function stake(
		uint256 tokenId,
		uint256 equipmentId,
		uint256 featureIndex,
		string memory equippedFor
	) external {
		require(_exists(tokenId), 'Token does not exist');
		require(ownerOf(tokenId) == msg.sender, 'Only owner can stake');
		require(!equipped[tokenId][equipmentId].isEquipped, 'already equipped');
		require(
			keccak256(bytes(featureNames[featureIndex])) ==
				keccak256((bytes(equippedFor))),
			'Staking for non-qty.'
		);
		uint256 slotIndex = IBaseEquipmentNFT(Equipment)
			.feature(equipmentId, SLOT)
			.updateValue;

		require(!_equippedAtSlot[tokenId][slotIndex], 'slot is occupied');

		equipped[tokenId][equipmentId] = EquippedMetadata({
			isEquipped: true,
			equippedAt: block.timestamp,
			equippedFor: equippedFor
		});
		_equippedAtSlot[tokenId][slotIndex] = true;
		_equippedIndex[tokenId][equipmentId] = _equipments[tokenId].length;
		_equipments[tokenId].push(equipmentId);

		IERC1155(Equipment).safeTransferFrom(
			msg.sender,
			address(this),
			equipmentId,
			1,
			''
		);
	}

	function unStake(uint256 tokenId, uint256 equipmentId) external {
		require(_exists(tokenId), 'Token does not exist');
		require(ownerOf(tokenId) == msg.sender, 'Only owner can un-stake');
		require(equipped[tokenId][equipmentId].isEquipped, 'not equipped');

		string[] memory featureNamesArr = new string[](numFeatures);
		uint256[] memory valuesArr = new uint256[](numFeatures);

		(featureNamesArr, valuesArr) = _getAttributes(tokenId, false);

		for (uint256 i = 0; i < numFeatures; i++) {
			values[tokenId][featureNamesArr[i]] = valuesArr[i];
		}

		uint256 slotIndex = IBaseEquipmentNFT(Equipment)
			.feature(equipmentId, SLOT)
			.updateValue;

		_equippedAtSlot[tokenId][slotIndex] = false;

		uint256 index = _equippedIndex[tokenId][equipmentId];

		_equipments[tokenId][index] = _equipments[tokenId][
			_equipments[tokenId].length - 1
		];
		_equippedIndex[tokenId][
			_equipments[tokenId][_equipments[tokenId].length - 1]
		] = index;

		_equipments[tokenId].pop();

		delete equipped[tokenId][equipmentId];

		IERC1155(Equipment).safeTransferFrom(
			address(this),
			msg.sender,
			equipmentId,
			1,
			''
		);
	}

	function getAttributes(uint256 tokenId)
		public
		view
		virtual
		override
		returns (string[] memory featureNamesArr, uint256[] memory valuesArr)
	{
		return _getAttributes(tokenId, true);
	}

	function _getAttributes(uint256 tokenId, bool addUpdateValue)
		internal
		view
		returns (string[] memory featureNamesArr, uint256[] memory valuesArr)
	{
		require(exists(tokenId), 'Query for non-existent token');
		uint256[] memory equipmentIds = _equipments[tokenId];
		if (equipmentIds.length == 0) {
			return super.getAttributes(tokenId);
		}
		featureNamesArr = new string[](numFeatures);
		valuesArr = new uint256[](numFeatures);
		uint256 currentTime = block.timestamp;
		for (uint256 i = 0; i < numFeatures; i++) {
			featureNamesArr[i] = featureNames[i];
			valuesArr[i] = values[tokenId][featureNamesArr[i]];

			for (uint256 index = 0; index < equipmentIds.length; index++) {
				uint256 stakingDuration;
				uint256 equippedAt = equipped[tokenId][equipmentIds[index]]
					.equippedAt;
				string memory equippedFor = equipped[tokenId][
					equipmentIds[index]
				].equippedFor;

				if (currentTime > equippedAt)
					stakingDuration = currentTime - equippedAt;

				IBaseEquipmentNFT.FeatureData
					memory featureData = IBaseEquipmentNFT(Equipment).feature(
						equipmentIds[index],
						featureNamesArr[i]
					);

				if (addUpdateValue) {
					valuesArr[i] += featureData.updateValue;
				}

				uint256 multiplier = stakingDuration.div(updateInterval);
				if (
					keccak256((bytes(equippedFor))) ==
					keccak256((bytes(featureNamesArr[i])))
				) {
					valuesArr[i] += featureData.dynamicValue * multiplier;
					console.log(valuesArr[i]);
				}
			}
		}
	}

	function getEquippedData(uint256 tokenId, uint256 equipmentId)
		external
		view
		override
		returns (EquippedMetadata memory)
	{
		return equipped[tokenId][equipmentId];
	}

	function setUpdateInterval(uint256 _updateInterval) external onlyAdmin {
		updateInterval = _updateInterval;
	}

	function _beforeTokenTransfer(
		address from,
		address to,
		uint256
	) internal view override {
		require(
			allowTransfersOnL2 ||
				(
					(allowedTransferAddresses[from] ||
						allowedTransferAddresses[to])
				) ||
				from == address(0),
			'Transfer prohibited'
		);
	}

	function setAllowedAddress(address addr, bool enabled) external onlyAdmin {
		allowedTransferAddresses[addr] = enabled;
	}

	function setAllowTransfersOnL2(bool allow) external onlyAdmin {
		allowTransfersOnL2 = allow;
	}
}



