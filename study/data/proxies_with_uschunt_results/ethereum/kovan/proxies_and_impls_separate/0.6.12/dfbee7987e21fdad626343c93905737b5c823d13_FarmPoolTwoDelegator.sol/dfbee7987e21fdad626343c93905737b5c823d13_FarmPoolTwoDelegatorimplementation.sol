// File: contracts/FarmPoolTwoDelegate.sol

pragma solidity 0.6.12;







contract FarmPoolTwoDelegate is FarmPoolTwoStorage {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event ClaimRevenue(address indexed user,uint256 indexed pid,address tokenAddress,uint256 tokenType,uint256 amout);
    event WeightSet(uint256 pid,uint256 newAllocationPoint); 
    event PoolAdded(address token,uint256 pid,uint256 allocationPoint,uint256 lastRewardBlockHeight,uint256 poolType);
    event DistributeTokenPerBlockSet(uint256 newDistributeTokenPerBlock);
    event HalvingPeriodSet(uint256 period);
    event UpdatePool(uint256 pid,uint256 distributeTokenAmount,uint256 updateBlockHeight);

    function initialize(
        address _distributeToken,
        uint256 _distributeTokenPerBlock,
        uint256 _halvingPeriod,
        uint256 _startBlock,
        uint256 _totalReward
    ) public {
        require(msg.sender == admin, "unautherized");
        require(distributeToken == address(0), "already initialized");
        require(_startBlock >= block.number, "invalid input");
        distributeToken = _distributeToken;
        distributeTokenPerBlock = _distributeTokenPerBlock;
        halvingPeriod = _halvingPeriod;
        startBlock = _startBlock;
        totalReward = _totalReward;
        fixEndBlock(false);
    }

    function setHalvingPeriod(uint256 _block) public {
        require(msg.sender == admin, "unautherized");
        halvingPeriod = _block;
        emit HalvingPeriodSet(_block);
    }

    function setFarmPoolOne(address _farmPoolOne) public {
        require(msg.sender == admin, "unautherized");
        farmPoolOne = _farmPoolOne;
    }

    // Set the number of distributeToken produced by each block
    function setDistributeTokenPerBlock(uint256 newPerBlock) public {
        require(msg.sender == admin, "unautherized");
        massUpdatePools();
        distributeTokenPerBlock = newPerBlock;
        emit DistributeTokenPerBlockSet(newPerBlock);
    }

    function poolLength() public view returns (uint256) {
        return poolInfo.length;
    }

    // Add a new lp to the pool. Can only be called by the owner.
    // XXX DO NOT add the same LP token more than once. Rewards will be messed up if you do.
    function add(
        uint256 _allocPoint,
        address _lpToken,
        bool _withUpdate
    ) public {
        require(msg.sender == admin, "unautherized");
        require(_lpToken != address(0), "_lpToken is the zero address");
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 lastRewardBlock = block.number > startBlock
            ? block.number
            : startBlock;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolInfo.push(
            PoolInfo({
                lpToken: _lpToken,
                allocPoint: _allocPoint,
                lastRewardBlock: lastRewardBlock,
                accDistributeTokenPerShare: 0,
                totalAmount: 0
            })
        );
        emit PoolAdded(_lpToken,poolInfo.length - 1,_allocPoint,lastRewardBlock,1);
    }

    // Update the given pool's distributeToken allocation point. Can only be called by the owner.
    function set(
        uint256 _pid,
        uint256 _allocPoint,
        uint256 _newPerBlock,
        bool _withUpdate
    ) public {
        require(msg.sender == admin, "unautherized");
        if (_withUpdate) {
            massUpdatePools();
        }
        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(
            _allocPoint
        );
        poolInfo[_pid].allocPoint = _allocPoint;
        distributeTokenPerBlock = _newPerBlock;
        emit WeightSet(_pid,_allocPoint);
        emit DistributeTokenPerBlockSet(_newPerBlock);
    }

    function phase(uint256 blockNumber) public view returns (uint256) {
        if (halvingPeriod == 0) {
            return 0;
        }
        if (blockNumber > startBlock) {
            return (blockNumber.sub(startBlock).sub(1)).div(halvingPeriod);
        }
        return 0;
    }

    function reward(uint256 blockNumber) public view returns (uint256) {
        uint256 _phase = phase(blockNumber);
        return distributeTokenPerBlock.div(2**_phase);
    }

    function getDistributeTokenBlockReward(uint256 _lastRewardBlock)
        public
        view
        returns (uint256)
    {
        uint256 blockReward = 0;
        uint256 rewardBlock = block.number > endBlock ? endBlock : block.number;
        if(rewardBlock <= _lastRewardBlock) return 0;
        uint256 n = phase(_lastRewardBlock);
        uint256 m = phase(rewardBlock);
        while (n < m) {
            n++;
            uint256 r = n.mul(halvingPeriod).add(startBlock);
            blockReward = blockReward.add(
                (r.sub(_lastRewardBlock)).mul(reward(r))
            );
            _lastRewardBlock = r;
        }
        blockReward = blockReward.add(
            (rewardBlock.sub(_lastRewardBlock)).mul(reward(rewardBlock))
        );
        return blockReward;
    }

    // Update reward variables for all pools. Be careful of gas spending!
    function massUpdatePools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            updatePool(pid);
        }
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 lpSupply;

        lpSupply = pool.totalAmount;
        if (lpSupply == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }

        uint256 blockReward = getDistributeTokenBlockReward(pool.lastRewardBlock);
        if (blockReward <= 0) {
            return;
        }
        uint256 distributeTokenReward = blockReward.mul(pool.allocPoint).div(
            totalAllocPoint
        );
        IISTARToken(distributeToken).mint(address(this), distributeTokenReward);
        issuedReward = issuedReward.add(distributeTokenReward);
        pool.accDistributeTokenPerShare = pool.accDistributeTokenPerShare.add(
            distributeTokenReward.mul(1e12).div(lpSupply)
        );

        pool.lastRewardBlock = block.number;
        emit UpdatePool(_pid,distributeTokenReward,block.number);
    }

    function pending(uint256 _pid, address _user)
        public
        view
        returns (uint256)
    {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accDistributeTokenPerShare = pool.accDistributeTokenPerShare;
        uint256 lpSupply = pool.totalAmount;
        if (user.amount > 0) {
            if (block.number > pool.lastRewardBlock) {
                uint256 blockReward = getDistributeTokenBlockReward(pool.lastRewardBlock);
                uint256 distributeTokenReward = blockReward.mul(pool.allocPoint).div(
                    totalAllocPoint
                );
                accDistributeTokenPerShare = accDistributeTokenPerShare.add(
                    distributeTokenReward.mul(1e12).div(lpSupply)
                );
                return
                    user.amount.mul(accDistributeTokenPerShare).div(1e12).sub(
                        user.rewardDebt
                    );
            }
            if (block.number == pool.lastRewardBlock) {
                return
                    user.amount.mul(accDistributeTokenPerShare).div(1e12).sub(
                        user.rewardDebt
                    );
            }
        }
        return 0;
    }

    // 复投接口
    function reDeposit(uint256 _amount, address user) public {
        require(msg.sender == farmPoolOne, "invalid sender");
        depositDistributeToken(0, _amount, user);
    }

    // Deposit LP tokens to BSCPool for distributeToken allocation.
    function deposit(uint256 _pid, uint256 _amount) public {
        require(_pid == 1, "invalid pid");
        depositDistributeToken(_pid, _amount, msg.sender);
    }

    function depositDistributeToken(
        uint256 _pid,
        uint256 _amount,
        address _user
    ) private {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        updatePool(_pid);
        if (user.amount > 0) {
            uint256 pendingAmount = user
                .amount
                .mul(pool.accDistributeTokenPerShare)
                .div(1e12)
                .sub(user.rewardDebt);
            if (pendingAmount > 0) {
                safeDistributeTokenTransfer(_user, pendingAmount);
                emit ClaimRevenue(_user,_pid,distributeToken,0,pendingAmount);
            }
        }
        if (_amount > 0) {
            IERC20(pool.lpToken).safeTransferFrom(
                msg.sender,
                address(this),
                _amount
            );
            user.amount = user.amount.add(_amount);
            pool.totalAmount = pool.totalAmount.add(_amount);
        }
        user.rewardDebt = user.amount.mul(pool.accDistributeTokenPerShare).div(1e12);
        emit Deposit(_user, _pid, _amount);
    }

    // Withdraw LP tokens from BSCPool.
    function withdraw(uint256 _pid, uint256 _amount) public {
        withdrawDistributeToken(_pid, _amount, msg.sender);
    }

    function withdrawDistributeToken(
        uint256 _pid,
        uint256 _amount,
        address _user
    ) private {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        require(user.amount >= _amount, "withdrawDistributeToken:insufficient amount");
        updatePool(_pid);
        uint256 pendingAmount = user
            .amount
            .mul(pool.accDistributeTokenPerShare)
            .div(1e12)
            .sub(user.rewardDebt);
        if (pendingAmount > 0) {
            safeDistributeTokenTransfer(_user, pendingAmount);
            emit ClaimRevenue(_user,_pid,distributeToken,0,pendingAmount);
        }
        if (_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.totalAmount = pool.totalAmount.sub(_amount);
            IERC20(pool.lpToken).safeTransfer(_user, _amount);
        }
        user.rewardDebt = user.amount.mul(pool.accDistributeTokenPerShare).div(1e12);
        emit Withdraw(_user, _pid, _amount);
    }

    // Safe distributeToken transfer function, just in case if rounding error causes pool to not have enough distributeTokens.
    function safeDistributeTokenTransfer(address _to, uint256 _amount) internal {
        uint256 distributeTokenBal = IISTARToken(distributeToken).balanceOf(address(this));
        if (_amount > distributeTokenBal) {
            IISTARToken(distributeToken).transfer(_to, distributeTokenBal);
        } else {
            IISTARToken(distributeToken).transfer(_to, _amount);
        }
    }

    function fixEndBlock(bool _withUpdate) public{
        require(msg.sender == admin, "unautherized");
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 restReward = totalReward.sub(issuedReward);
        uint256 blockHeightEnd = getEndBlock(restReward);
        endBlock = blockHeightEnd;
    }
    
    function getEndBlock(uint256 restReward) internal view returns(uint256 blockHeightEnd){
        uint256 blockHeightBegin = block.number > startBlock ? block.number : startBlock;
        blockHeightEnd = endBlock;
        while(restReward != 0){
         (blockHeightEnd,restReward)= _getEndBlock(blockHeightBegin,restReward);
          blockHeightBegin = blockHeightEnd;
        }
        return blockHeightEnd;
    }
    function _getEndBlock(uint256 blockHeightBegin, uint256 rewardPending)internal view returns(uint256 blockHeightEnd,uint256 rewardRest){
        uint256 _phase = blockHeightBegin.sub(startBlock).mod(halvingPeriod) == 0 && blockHeightBegin > startBlock ? phase(blockHeightBegin)+1 : phase(blockHeightBegin);
        uint256 endRewardBlock = (_phase + 1).mul(halvingPeriod).add(startBlock);
        require(distributeTokenPerBlock.div(2 ** _phase) > 0,"error");
        uint256 _reward;
        _reward = endRewardBlock.sub(blockHeightBegin).mul(distributeTokenPerBlock.div(2 ** _phase));      
        if(rewardPending >= _reward)
            return(endRewardBlock,rewardPending.sub(_reward));
         else
            return(blockHeightBegin.add(rewardPending.div(distributeTokenPerBlock.div(2 ** _phase))),0);
    }
}