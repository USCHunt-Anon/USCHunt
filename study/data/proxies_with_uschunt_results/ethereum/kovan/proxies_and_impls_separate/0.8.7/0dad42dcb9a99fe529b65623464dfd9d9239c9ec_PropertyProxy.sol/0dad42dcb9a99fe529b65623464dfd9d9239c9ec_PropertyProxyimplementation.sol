pragma solidity ^0.8.7;

contract tefff{
    address public nn;
    mapping(address=>mmPar) public mm;
    struct mmPar{
        uint gg;
        uint ff;
        address aa;
        address bb;
        address cc;
    }
    address public aa;
    address public bb;
    address public cc;

    function setmmaa(address add) public {
        mm[add].aa = add;
    }
    function setmmbb(address add) public {
        mm[add].bb = add;
    }
    function setmmcc(address add) public {
        mm[add].cc = add;
    }

    function setaa(address addr) public {
        aa = addr;
    }

    function setbb(address addr) public {
        bb = addr;
    }

    function setcc(address addr) public {
        cc = addr;
    }
}