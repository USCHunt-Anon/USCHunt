pragma solidity ^0.8.4;









contract GAIA_NFT is ERC721Upgradeable, OwnableUpgradeable, ReentrancyGuardUpgradeable, EIP712Upgradeable {
    
    uint256 private MaxOriginalTokens;
    string  private _baseTokenURI;
    bytes32 private _BREEDING_TYPEHASH;
    bytes32 private _MINT_TYPEHASH;
    string private constant NAME = "GAIA NFT";
    string private constant SYMBOL = "GAIA";
    string private constant VERSION = "1";
    uint256 private _totalOriginalTokens;
    uint256 private _totalNonOriginalTokens;
    mapping(address => uint256) private _nonces;
    address private _validator;
    // address -> nft -> token -> amount
    mapping(address => mapping(uint256 => mapping(uint256 =>uint256))) private _accessories;

    event Breeded(address indexed creator, uint256 tokenId);
    event UserRegistered(address indexed wallet);
    event ValidatorChanged(address indexed wallet);
    event takenOn(address indexed, uint256 indexed nft, uint256 indexed token, uint256 amount);
    event takenOff(address indexed, uint256 indexed nft, uint256 indexed token, uint256 amount);

    function initialize() initializer  public {
        _baseTokenURI="";
        _totalOriginalTokens=0;
        _totalNonOriginalTokens=0;
        __ERC721_init(NAME,SYMBOL);
        __Ownable_init();
        __EIP712_init(NAME, VERSION);
        __ReentrancyGuard_init();
        MaxOriginalTokens = 16128;
        _BREEDING_TYPEHASH = keccak256("Breeding(address creator,uint256 nft_1,uint256 nft_2,address token1,uint256 cost1,address token2,uint256 cost2,uint256 nonce,uint256 deadline)");
        _MINT_TYPEHASH = keccak256("Mint(address creator,uint256 tokenId,uint256 nonce,uint256 deadline)");
    }
    function setValidator(address wallet)external onlyOwner{
        _validator=wallet;
        emit ValidatorChanged(wallet);
    }

    function mintTo(address to, uint256 tokenId) public onlyOwner {
        require(tokenId < MaxOriginalTokens, "Mint: Illegal token ID");
        _mint(to, tokenId);
        _totalOriginalTokens+=1;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        return  string(abi.encodePacked(super.tokenURI(tokenId), ".json" ));
    }

    function _baseURI() internal view override virtual returns (string memory) {
        return  _baseTokenURI;
    }
    function setBaseURI(string memory uri) external onlyOwner {
        _baseTokenURI=uri;
    }
    function breedingPermit(uint256 nft_1,uint256 nft_2,address token1,uint256 cost1,address token2,uint256 cost2, uint256 deadline, uint8 v, bytes32 r, bytes32 s) external {
        require(block.timestamp <= deadline, "Breeding: expired deadline");
        bytes32 hashStruct = keccak256(
             abi.encode(
                 _BREEDING_TYPEHASH,
                 msg.sender,
                 nft_1,
                 nft_2,
                 token1,
                 cost1,
                 token2,
                 cost2,
                 _useNonce(msg.sender),
                 deadline                 
             )
         );
        bytes32 hash = _hashTypedDataV4(hashStruct);
        address signer = ECDSAUpgradeable.recover(hash, v, r, s);
        require(signer == _validator, "Breeding: invalid signature");
        uint256 newTokenId=MaxOriginalTokens + _totalNonOriginalTokens;
        IERC20Upgradeable(token1).transferFrom(msg.sender,address(0), cost1);
        IERC20Upgradeable(token2).transferFrom(msg.sender,address(0), cost2);
        _mint(msg.sender,newTokenId);
        _totalNonOriginalTokens+=1;
        emit Breeded(msg.sender, newTokenId);
    }

    function mintPermit(address creator, uint256 tokenId,uint256 deadline,
        uint8 v, bytes32 r, bytes32 s) external {
        require(block.timestamp <= deadline, "Mint NFT: expired deadline");
        bytes32 hashStruct = keccak256(
             abi.encode(
                 _MINT_TYPEHASH,
                 msg.sender,
                 tokenId,
                 _useNonce(msg.sender),
                 deadline                 
             )
         );
        bytes32 hash = _hashTypedDataV4(hashStruct);
        address signer = ECDSAUpgradeable.recover(hash, v, r, s);
        require(signer == _validator, "Mint NFT: invalid signature");
        _mint(creator,tokenId);
        _totalOriginalTokens+=1;
    }

    function takeOn(address accessories, uint256 nft, uint256 token, uint256 amount ) external nonReentrant{
        IERC1155Upgradeable(accessories).safeTransferFrom(msg.sender, address(this), token, amount,"");
        require( IERC1155Upgradeable(accessories).balanceOf(msg.sender, token) >= amount, "TakeOn: no enough accessories");
        _accessories[msg.sender][nft][token]+=amount;
        emit takenOn(msg.sender, nft, token, amount);
    }
    function takeOff(address accessories, uint256 nft, uint256 token, uint256 amount ) external nonReentrant{
        require(_accessories[msg.sender][nft][token] >= amount, "Illegal accessories amount");
        IERC1155Upgradeable(accessories).safeTransferFrom(address(this), msg.sender,token, amount,"");
        _accessories[msg.sender][nft][token]-=amount;
        emit takenOff(msg.sender, nft, token, amount);
    }
    function nonces(address owner) public view virtual returns (uint256) {
        return _nonces[owner];
    }
    function _useNonce(address owner) internal virtual returns (uint256 current) {
        current = _nonces[owner];
        _nonces[owner]++;
    }
    function totalSupply() public view virtual returns (uint256) {
        return _totalOriginalTokens + _totalNonOriginalTokens;
    }
    

}