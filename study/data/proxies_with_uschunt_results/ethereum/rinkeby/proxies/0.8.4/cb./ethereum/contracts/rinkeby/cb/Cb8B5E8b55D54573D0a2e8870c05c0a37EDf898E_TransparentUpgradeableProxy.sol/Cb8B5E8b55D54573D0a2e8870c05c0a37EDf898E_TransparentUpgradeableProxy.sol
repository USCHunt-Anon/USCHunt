// File: DogERC721Upgradeable.sol


pragma solidity ^0.8.4;







contract DogERC721 is  ERC721EnumerableUpgradeable, AccessControlUpgradeable, ReentrancyGuardUpgradeable,EIP712Upgradeable
{
    mapping(address => uint256) private _nonces;
    mapping(uint256 => address) private _creators;
    mapping( address => uint) private _whitelist;
    uint private _preSaleOn = 0;
    uint private _saleOn = 0;
    string private _defaultUri;
    uint constant MAX_TOKENS = 10000;
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant SETURL_ROLE = keccak256("SETURL_ROLE");

    bytes32 constant _MINT_TYPEHASH = keccak256("Mint(address spender,uint256 id,uint256 price,uint256 nonce,uint256 deadline)");
    string private constant NAME = "CryptoDogs";
    string private constant SYMBOL = "CDOG";
    string private constant VERSION = "1";
    string private _baseExtention;
    constructor(){
     }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        return string(abi.encodePacked(super.tokenURI(tokenId),_baseExtention));
    }

    function initialize() initializer public {
        __ERC721_init(NAME,SYMBOL);
        __AccessControl_init();
        __ReentrancyGuard_init();
        __EIP712_init(NAME, VERSION);
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(SETURL_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);
     }
    function _baseURI() internal view override virtual returns (string memory) {
        return _defaultUri;
    }

    function setDefaultUri(string memory uri)external onlyRole(SETURL_ROLE){
        _defaultUri = uri;
    }
    function setBaseExtention(string memory extention)external onlyRole(SETURL_ROLE){
        _baseExtention = extention;
    }
    function getBaseExtention()external view returns(string memory){
        return _baseExtention;
    }
    
    function mint(
        address to,
        uint256 id,
        uint256 price,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external payable  nonReentrant{
        require( totalSupply() < MAX_TOKENS, "Max tokens is reached" );
        require( msg.value >= price, "No enough funds for mint");
        require( block.timestamp <= deadline, "Expired deadline");
        bytes32 hashStruct = keccak256(
             abi.encode(
                 _MINT_TYPEHASH,
                 to,
                 id,
                 price,
                 _useNonce(msg.sender),
                 deadline                 
             )
         );
        bytes32 hash = _hashTypedDataV4(hashStruct);
        address signer = ECDSAUpgradeable.recover(hash, v, r, s);
        require(hasRole ( MINTER_ROLE, signer), "ERC20Permit: invalid signature");        
        _mint(to, id);
        _creators[id] = to;
    }
    function giftMint(
        address _to,
        uint256 _id
    ) external  onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        require( totalSupply() < MAX_TOKENS, "Max tokens is reached" );
        _mint(_to, _id);
        _creators[_id] = _to;
    }
    function getCreator(uint256 _id) external view returns(address){
        return _creators[_id];
    }

    function nonces(address owner) external view virtual returns (uint256) {
        return _nonces[owner];
    }
    function _useNonce(address owner) internal virtual returns (uint256 current) {
        current = _nonces[owner];
        _nonces[owner]++;
    }

    function withdraw(address payable to, uint256 amount) external onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant{
        require(to != address(0), "Zero address prohibited");
        Address.sendValue(to, amount);
    }
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(  ERC721EnumerableUpgradeable, AccessControlUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

}