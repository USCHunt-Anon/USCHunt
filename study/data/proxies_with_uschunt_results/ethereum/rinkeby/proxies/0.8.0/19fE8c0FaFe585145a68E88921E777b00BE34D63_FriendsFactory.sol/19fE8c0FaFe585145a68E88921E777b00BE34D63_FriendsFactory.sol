contract FriendsFactory {
    event WalletCreated(string name, address safe);

    address public immutable GNOSIS_SAFE_TEMPLATE_ADDRESS;

    //Will this deploy with the whole contract code for GnosisSafeProxyFactory.sol?
    GnosisSafeProxyFactory public immutable GNOSIS_SAFE_PROXY_FACTORY;
    address public immutable GNOSIS_SAFE_FALLBACK_HANDLER;

    constructor(
        address _safeTemplateAddress,
        address _safeProxyFactoryAddress,
        address _safeFallbackHandlerAddress
    ) {
        GNOSIS_SAFE_TEMPLATE_ADDRESS = _safeTemplateAddress;
        GNOSIS_SAFE_PROXY_FACTORY = GnosisSafeProxyFactory(
            _safeProxyFactoryAddress
        );
        GNOSIS_SAFE_FALLBACK_HANDLER = _safeFallbackHandlerAddress;
    }

    function createFriendGroup(string memory _name, address[] memory _addresses)
        external
    {
        bytes32 salt = keccak256(abi.encode(_name, _addresses));

        //Let me know if this is stupid. I wanted to avoid making Interfaces but maybe that's the pattern for proxies
        bytes4 FUNC_SELECTOR = bytes4(
            keccak256(
                "setup(address[],uint256,address,bytes,address,address,uint256,address)"
            )
        );

        bytes memory data = abi.encodeWithSelector(
            FUNC_SELECTOR,
            _addresses,
            1,
            address(0),
            "",
            address(GNOSIS_SAFE_FALLBACK_HANDLER),
            address(0),
            0,
            payable(0)
        );

        GnosisSafeProxy proxy = GNOSIS_SAFE_PROXY_FACTORY.createProxyWithNonce(
            GNOSIS_SAFE_TEMPLATE_ADDRESS,
            // data,
            "",
            uint256(salt)
        );

        //Create safe
        (bool success, bytes memory returnData) = address(proxy).staticcall(data);

        if(success) {
            //emit event
            emit WalletCreated(_name, address(proxy));
        }

        // emit WalletCreated(_name, address(proxy));
    }
}

