pragma solidity 0.6.12;










contract Operator is Initializable, ReentrancyGuardUpgradeable, Ownable {
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    using SafeERC20 for IERC20;
    using Address for address;

    uint256 public constant SET_LIMIT = 256;

    address public vault;
    IERC20 public usx;
    IERC20 public df;

    EnumerableSetUpgradeable.AddressSet internal strategies;
    EnumerableSetUpgradeable.AddressSet internal makers;
    EnumerableSetUpgradeable.AddressSet internal traders;

    EnumerableSetUpgradeable.AddressSet internal whitelist;

    event NewVault(address indexed oldVault, address indexed newVault);

    event StrategyAdded(address indexed strategy);
    event StrategyRemoved(address indexed strategy);

    event MakerAdded(address indexed maker);
    event MakerRemoved(address indexed maker);

    event TraderAdded(address indexed trader);
    event TraderRemoved(address indexed trader);

    event WhitelistAdded(address indexed whitelist);
    event WhitelistRemoved(address indexed whitelist);

    constructor(
        IERC20 _usx,
        IERC20 _df,
        address _vault
    ) public {
        initialize(_usx, _df, _vault);
    }

    /**
     * @notice Initializes the contract.
     */
    function initialize(
        IERC20 _usx,
        IERC20 _df,
        address _vault
    ) public initializer {
        _initialize(_usx, _df, _vault);
    }

    function _initialize(
        IERC20 _usx,
        IERC20 _df,
        address _vault
    ) internal {
        require(address(_usx) != address(0), "USX cannot be the zero address");
        require(address(_df) != address(0), "DF cannot be the zero address");

        __Ownable_init();
        __ReentrancyGuard_init();

        usx = _usx;
        df = _df;

        _setVaultInternal(_vault);
    }

    /**
     * @dev Set the vault contract to borrow/repay USX.
     */
    function _setVaultInternal(address _newVault) internal {
        require(_newVault != address(0), "Vault cannot be the zero address");

        address _oldVault = vault;
        vault = _newVault;

        emit NewVault(_oldVault, _newVault);
    }

    /**
     * @notice Set the vault where USX can be borrowed.
     * @param _vault The address of the _vault.
     */
    function _setVault(address _vault) external onlyOwner nonReentrant {
        _setVaultInternal(_vault);
    }

    /**
     * @notice Adds a new strategy.
     * @param _strategy The strategy to add.
     */
    function _addStrategy(address _strategy) external onlyOwner nonReentrant {
        require(
            IStrategy(_strategy).isStrategy(),
            "Invalid strategy contract address"
        );
        require(strategies.length() < SET_LIMIT, "Strategy limit reached");

        // add() will return false if the set already contains the _strategy.
        require(strategies.add(_strategy), "Strategy has already been added");

        emit StrategyAdded(_strategy);
    }

    /**
     * @notice Remove a strategy.
     * @param _strategy The strategy to remove.
     */
    function _removeStrategy(address _strategy)
        external
        onlyOwner
        nonReentrant
    {
        // remove() will return false if the set does not contain the _strategy.
        require(strategies.remove(_strategy), "Strategy does not exist");

        emit StrategyRemoved(_strategy);
    }

    /**
     * @notice Adds a new maker.
     * @param _maker The maker to add.
     */
    function _addMaker(address _maker) external onlyOwner nonReentrant {
        require(IMaker(_maker).isMaker(), "Invalid maker contract address");
        require(makers.length() < SET_LIMIT, "Maker limit reached");

        // add() will return false if the set already contains the _maker.
        require(makers.add(_maker), "Maker has already been added");

        _maker.functionDelegateCall(abi.encodeWithSignature("activate()"));

        emit MakerAdded(_maker);
    }

    /**
     * @notice Remove a maker.
     * @param _maker The maker to remove.
     */
    function _removeMaker(address _maker) external onlyOwner nonReentrant {
        // remove() will return false if the set does not contain the _maker.
        require(makers.remove(_maker), "Maker does not exist");

        _maker.functionDelegateCall(abi.encodeWithSignature("deactivate()"));

        emit MakerRemoved(_maker);
    }

    /**
     * @notice Adds a new trader.
     * @param _trader The trader to add.
     */
    function _addTrader(address _trader) external onlyOwner nonReentrant {
        require(ITrader(_trader).isTrader(), "Invalid trader contract address");
        require(traders.length() < SET_LIMIT, "Trader limit reached");

        // add() will return false if the set already contains the _trader.
        require(traders.add(_trader), "Trader has already been added");

        _trader.functionDelegateCall(abi.encodeWithSignature("activate()"));

        emit TraderAdded(_trader);
    }

    /**
     * @notice Remove a trader.
     * @param _trader The trader to remove.
     */
    function _removeTrader(address _trader) external onlyOwner nonReentrant {
        // remove() will return false if the set does not contain the _trader.
        require(traders.remove(_trader), "Trader does not exist");

        _trader.functionDelegateCall(abi.encodeWithSignature("deactivate()"));

        emit TraderRemoved(_trader);
    }

    /**
     * @notice Adds a new whitelist user.
     * @param _whitelist The whitelist user to add.
     */
    function _addWhitelist(address _whitelist) external onlyOwner nonReentrant {
        require(
            address(_whitelist) != address(0),
            "Invalid whitelist user contract address"
        );
        require(whitelist.length() < SET_LIMIT, "Whitelist limit reached");

        // add() will return false if the set already contains the _whitelist user.
        require(
            whitelist.add(_whitelist),
            "Whitelist user has already been added"
        );

        emit WhitelistAdded(_whitelist);
    }

    /**
     * @notice Remove a whitelist.
     * @param _whitelist The whitelist to remove.
     */
    function _removeWhitelist(address _whitelist)
        external
        onlyOwner
        nonReentrant
    {
        // remove() will return false if the set does not contain the _whitelist.
        require(whitelist.remove(_whitelist), "Whitelist does not exist");

        emit WhitelistRemoved(_whitelist);
    }

    /**
     * @dev According to the strategy index and encoded data to execute a series of actions.
     * @param _strategyIndex Strategy index.
     *          0: borrow USX, swap to get DF, add liquidity;
     *          1: swap to get USX, remove liquidity, repay USX;
     * @param _vaultData Encoded data for vault, typically the target address and the borrow/repay amount.
     * @param _traderData Encoded data for trading, different platforms need different parameter types
     *        and number of parameters, so just pass the encoded data, and then decode data to execute.
     *        Including the index of the trader strategy and the encoded parameters for swapping.
     * @param _makerData Encoded data for making. Including the index of the maker strategy and the encoded
     *        parameters for adding/removing liquidity.
     */
    function executeStrategy(
        uint256 _strategyIndex,
        bytes memory _vaultData,
        bytes memory _traderData,
        bytes memory _makerData
    ) external nonReentrant {
        require(
            whitelist.contains(msg.sender),
            "Only whitelisted users can execute strategies"
        );

        require(
            _strategyIndex < strategies.length(),
            "Strategy index out of range"
        );

        strategies.at(_strategyIndex).functionDelegateCall(
            abi.encodeWithSignature(
                "execute(bytes,bytes,bytes)",
                _vaultData,
                _traderData,
                _makerData
            )
        );
    }

    function getAddresses(
        EnumerableSetUpgradeable.AddressSet storage _addressSet
    ) internal view returns (address[] memory _addresses) {
        uint256 _len = _addressSet.length();
        _addresses = new address[](_len);
        for (uint256 i = 0; i < _len; i++) {
            _addresses[i] = _addressSet.at(i);
        }
    }

    function getWhitelist() external view returns (address[] memory) {
        return getAddresses(whitelist);
    }

    function getStrategies() external view returns (address[] memory) {
        return getAddresses(strategies);
    }

    function getMakers() external view returns (address[] memory) {
        return getAddresses(makers);
    }

    function getTraders() external view returns (address[] memory) {
        return getAddresses(traders);
    }

    // Function to receive Ether
    receive() external payable {}
}