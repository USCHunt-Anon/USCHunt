//import"./interfaces/IUninterestedUnicorns.sol";
//import"./UniCandy.sol";

//import"hardhat/console.sol";

contract UniQuest is AccessControlEnumerableUpgradeable {
    using SafeMathUpgradeable for uint256;
    using CountersUpgradeable for CountersUpgradeable.Counter;

    struct Quest {
        address questOwner;
        uint256 questStart;
        uint256 questEnd;
        uint256 lastClaim;
        uint256 clanMultiplier;
        uint256 rareMultiplier;
        uint8[] unicornIds;
        QuestState questState;
    }

    struct ClanCounter {
        uint8 airCount;
        uint8 earthCount;
        uint8 fireCount;
        uint8 waterCount;
        uint8 darkCount;
        uint8 pureCount;
    }

    // Enums
    enum Clans {
        AIR,
        EARTH,
        FIRE,
        WATER,
        DARK,
        PURE
    }

    enum QuestState {
        IN_PROGRESS,
        ENDED
    }

    IUninterestedUnicorns public UU;

    uint256 public DENOMINATOR;

    uint256 public baseReward;
    uint256[] public clanMultipliers;
    uint256[] public rareMultipliers;
    bytes public clans;

    mapping(uint256 => Quest) public quests;
    mapping(uint256 => uint256) public onQuest; // Maps tokenId to QuestId (0 = not questing)
    mapping(uint256 => uint256) clanCounter;
    // mapping(uint256 => uint256) public clans;

    // Private Variables
    CountersUpgradeable.Counter private _questId;
    bool private initialized;

    // Reserve Storage
    uint256[50] private ______gap;

    // Modifiers
    modifier onlyAdmin() {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, _msgSender()),
            "UninterestedUnicorns: OnlyAdmin"
        );
        _;
    }

    // constructor(
    //     address uu,
    //     uint256 _baseReward,
    //     address treasury
    // ) {
    //     _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    //     grantRole(DEFAULT_ADMIN_ROLE, treasury);
    //     baseReward = _baseReward;
    //     UU = IUninterestedUnicorns(uu);
    //     DENOMINATOR = 10000;
    //     clanMultipliers = [0, 0, 400, 600, 800, 1000];
    //     rareMultipliers = [0, 200, 400, 600, 800, 1000];
    // }

    function __UniQuest_init(
        address uu,
        uint256 _baseReward,
        address treasury
    ) public initializer {
        require(!initialized, "Contract instance has already been initialized");
        __AccessControlEnumerable_init();

        // Constructor init
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        grantRole(DEFAULT_ADMIN_ROLE, treasury);
        baseReward = _baseReward;
        UU = IUninterestedUnicorns(uu);
        DENOMINATOR = 10000;
        clanMultipliers = [0, 0, 400, 600, 800, 1000];
        rareMultipliers = [0, 200, 400, 600, 800, 1000];

        initialized = true;
    }

    function startEasyQuest(uint8[] calldata unicornIds) public {
        require(
            areAvailiable(unicornIds),
            "UniQuest: One or More Unicorns are already on a Quest"
        );

        require(
            areOwned(unicornIds),
            "UniQuest: One or More Unicorns are not owned by you!"
        );

        _questId.increment();
        uint256 newQuestId = _questId.current();

        callLockMany(unicornIds);

        for (uint256 i = 0; i < unicornIds.length; i++) {
            onQuest[unicornIds[i]] = newQuestId;
        }

        uint256 _clanMultiplier;
        uint256 _rareMultiplier;

        (_clanMultiplier, _rareMultiplier) = calculateMultipliers(unicornIds);

        Quest memory _quest = Quest(
            msg.sender,
            block.timestamp, // quest start
            block.timestamp.add(30 days), // quest end
            block.timestamp, // last claim
            _clanMultiplier,
            _rareMultiplier,
            unicornIds,
            QuestState.IN_PROGRESS
        );

        quests[newQuestId] = _quest;
    }

    // function startMediumQuest(uint256[] calldata unicornIds) public {
    //     uint256 duration = 90 days;
    // }

    // function startHardQuest(uint256[] calldata unicornIds) public {
    //     uint256 duration = 180 days;
    // }

    function endQuest(uint256 questId) public {
        // Only Quest Owner
        require(
            msg.sender == quests[questId].questOwner,
            "UniQuest: Not the owner of quest"
        );
        // Current Time > Quest End Time
        require(
            block.timestamp > quests[questId].questEnd,
            "UniQuest: Unicorns are still questing!"
        );
        // Must be questing state
        require(
            quests[questId].questState == QuestState.IN_PROGRESS,
            "UniQuest: Quest already Ended"
        );

        callUnlockMany(quests[questId].unicornIds);
        quests[questId].questState;
    }

    /// @dev Lock UU for transfer
    function callLockMany(uint8[] calldata tokenIds) internal {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            UU.lockToken(tokenIds[i]);
        }
    }

    /// @dev Unlock UU for transfer, used at quest end
    function callUnlockMany(uint8[] memory tokenIds) internal {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            UU.unlockToken(tokenIds[i]);
        }
    }

    function getClanMultiplier(uint256 clanCount)
        public
        view
        returns (uint256)
    {
        return clanMultipliers[clanCount];
    }

    function getRareMultiplier(uint256 rareCount)
        public
        view
        returns (uint256)
    {
        return rareMultipliers[rareCount];
    }

    /// @dev Calculates Clan Multiplier based on tokenIds
    function calculateMultipliers(uint8[] calldata _tokenIds)
        internal
        view
        returns (uint256 _clanMultiplier, uint256 _rareMultiplier)
    {
        uint8[6] memory _clanCounter = [0, 0, 0, 0, 0, 0];
        uint8 maxCount = 0;
        uint8 maxIndex;
        uint256 rareCount = 0;

        /*
        Example:
        Fire, Fire, Pure, Water, Dark

        Clan bonus

        */

        // Count UU per clan
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            _clanCounter[getClan(_tokenIds[i]) - 1] += 1;
        }

        // Find Maximum Count and Index of Max Count
        for (uint8 i = 0; i < _clanCounter.length; i++) {
            if (_clanCounter[i] > maxCount) {
                maxCount = _clanCounter[i];
                maxIndex = i;
            }
        }

        // Wildcard bonus
        if (maxIndex < 4) {
            maxCount += _clanCounter[5];
        }

        _clanMultiplier = getClanMultiplier(maxCount);

        rareCount = rareCount.add(_clanCounter[4]).add(_clanCounter[5]);
        _rareMultiplier = getRareMultiplier(rareCount);
    }

    function calculateRewards(uint256 questId)
        public
        view
        returns (uint256 rewardAmount)
    {
        Quest memory quest = quests[questId];
        uint256 lastClaim = quest.lastClaim;
        uint256 duration = block.timestamp.sub(lastClaim);
        uint256 numUU = quest.unicornIds.length;
        uint256 clanMultiplier = quest.clanMultiplier;
        uint256 rareMultiplier = quest.rareMultiplier;

        rewardAmount = baseReward
            .div(1 days)
            .mul(duration)
            .mul(numUU)
            .mul(clanMultiplier.add(DENOMINATOR))
            .div(DENOMINATOR)
            .mul(rareMultiplier.add(DENOMINATOR))
            .div(DENOMINATOR);
        console.log(rewardAmount);
    }

    /// @dev Determines if the tokenIds are availiable for questing
    function areAvailiable(uint8[] calldata tokenIds)
        public
        view
        returns (bool out)
    {
        out = true;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            if (onQuest[tokenIds[i]] > 0) {
                out = false;
            }
        }
    }

    /// @dev Determines if the all tokenIds are owned by msg sneder
    function areOwned(uint8[] calldata tokenIds)
        public
        view
        returns (bool out)
    {
        out = true;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            if (UU.ownerOf(tokenIds[i]) != msg.sender) {
                out = false;
            }
        }
    }

    function setBaseReward(uint256 _amount) public onlyAdmin {
        baseReward = _amount;
    }

    /// @dev Storing Clan Metadata as 1 byte hexes on a byte for gas optimization
    function updateClans(bytes calldata _clans) public onlyAdmin {
        clans = _clans;
    }

    function getClan(uint256 tokenId) public view returns (uint8) {
        return uint8(clans[tokenId - 1]);
    }
}


