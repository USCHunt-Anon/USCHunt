contract InverseV0  is InverseV0Events, Ownable, ReentrancyGuard{

    using StakingLibrary for StakingLibrary.StakingDetails;
    using StakingLibrary for StakingLibrary.StakingReward;
    using BettingLibrary for BettingLibrary.BettingDetails;


    bool internal isInitialized; 
    uint256  public miniStakeAmount ;   // Min amount of token that user can stake 
    address  public rootNode;           // Owner of this contract
    uint256  public tokenStakedAmount;  // Total Amount staked in contract
    uint256  DAYS_IN_SECONDS;           // Seconds in a day
    uint256  betFactorLP;               // this is the ratio according to which users can bet considering the amount staked..
    uint256  miniBetAmount;             // min amount that user can bet on.
    uint256  maxBetAmount;              // max amount that user can bet on.
    uint256  flexibleDaysCounter;       // Number of flexible plans
    uint256  defiCoinsCounter;          // Number of Defi plans
    uint256  chainCoinsCounter;         // Number of Chain plans      
    uint256  NFTCoinsCounter;           // Number of NFT plans
    uint256  planDaysInFixed;
    uint256  dropValueFixed;
    uint256  rewardFixed;
    uint256 offset ; 
    uint256 stakerIncentiveCounter;
    address oracleAddress;
    

    IERC20 XIV;
    OracleWrapper oracle;

    // Fixed => 1, Flexible =>2;
    // Defi coins => 1, Chain coins => 2, NFT coins => 3;

    mapping(address => StakingLibrary.StakingDetails) public stake;
    mapping(address =>  uint256) public betCounter;
    mapping(address => bool)coinStatus;
    mapping(uint256 => uint256) public planDaysInFlexible;
    mapping(uint => mapping(uint => mapping(uint => address))) public coins;
    mapping(address => mapping(uint => mapping(address => bool)))isEligibleForBet;
    mapping(address => mapping(uint256 => BettingLibrary.BettingDetails)) public userBets;
    mapping(uint256 => StakingLibrary.StakingReward) stakerRewards; 
    
    uint32 []  dropValueFlexible;
    uint32 []  rewardFlexible;
    
    
    
   

    function initialize(address _rootNode, address _XIVAddress, uint _miniStakeAmount , uint _betFactorLP, uint _miniBetAmount, uint _maxBetAmount, address _oracle) public {
        require(!isInitialized, "Initailized Once");
        isInitialized = true;
        rootNode = _rootNode;
        miniStakeAmount  = _miniStakeAmount ; 
        betFactorLP = _betFactorLP;
        miniBetAmount = _miniBetAmount;
        maxBetAmount = _maxBetAmount;
        XIV = IERC20(_XIVAddress);
        oracle = OracleWrapper(_oracle);
        DAYS_IN_SECONDS = 86400; 
        rewardFixed = 100;
        dropValueFixed = 10;
        planDaysInFixed = 1;
        dropValueFlexible = [3,5,7,9,10,12,14,15];
        rewardFlexible = [3,5,7,10,20,40,70,100];
        planDaysInFlexible[1] = 1;
        planDaysInFlexible[2] = 3;
        planDaysInFlexible[3] = 7;
        flexibleDaysCounter = 4; 
    }

    modifier validateBetArguments (uint amount, address coinAddress , uint planType)  {
        require(coinStatus[coinAddress], "Not active");
        require(!isEligibleForBet[msg.sender][planType][coinAddress], "Not allowed for this currency");
       
        // Condition to check and testcase
        require((betFactorLP * tokenStakedAmount) >= ((XIV.balanceOf(address(this)) - tokenStakedAmount) + amount), "Betfactor exceeded");        
        require(amount >= miniBetAmount && amount <= maxBetAmount ,"Please enter amount in the specified range");   
        _;
    }

    modifier onlyOwnerAccess () {
        require(msg.sender == rootNode, "Only owner");
        _;
    }

    modifier checkAmount (uint amount, uint value, bool equalOrGreater, address user) {
        
        if(stake[user].stakedTokensWithRewards != uint128(0)){
            require((stake[user].lastIndex == stakerIncentiveCounter), "First claim your bets");
        }
        
        equalOrGreater ? require(amount >= value, "Insufficient amount") : require(amount > value, "Zero amount") ;
        _;
    }

    function resolveBet (uint index, address user, uint result) public onlyOwnerAccess {
        require((index > uint(0)) && (index <= betCounter[user]) ,"Invalid bet index");
        BettingLibrary.BettingDetails storage betDetails = userBets[user][index];


        //Check
        // require(block.timestamp > betDetails.endTime , "Bet can'end");
        
        if(result == 2){
            stakerRewards[stakerIncentiveCounter].incentiveStakingDetails(block.timestamp, betDetails.amount);
            stakerIncentiveCounter++;
        }

        //Find the result
        betDetails.declareBet(result);
        isEligibleForBet[user][betDetails.planType][betDetails.coinAddress] = false;
        
    }

    // function claimIncentiveStake () external {
    //     require((stake[msg.sender].stakingTime != uint64(0)), "Not staked yet");
    //     require((stake[msg.sender].lastIndex == stakerIncentiveCounter), "No reward to claim");
    //     uint incentive;
    //     for(uint i = stake[msg.sender].lastIndex; i <=  stakerIncentiveCounter; i++){
    //         if(stake[msg.sender].stakingTime  < stakerRewards[i].bettingEndTime) {
    //             incentive += (stake[msg.sender].stakedTokensWithRewards/tokenStakedAmount)*stakerRewards[i].stakingReward;
    //             if(i == stakerIncentiveCounter){
    //                 stake[msg.sender].lastIndex = stakerIncentiveCounter;
    //             }
    //         }
    //     }

    // }

    function claimABet () external {
        require(betCounter[msg.sender] > 0, "No Bet yet");
        uint amount;
       
        for(uint i = 1; i <= betCounter[msg.sender]; i++){
            BettingLibrary.BettingDetails storage betDetails = userBets[msg.sender][i];
            if((betDetails.status == uint(1) && (!betDetails.claimed))){
                amount += betDetails.amount;
                amount += ((betDetails.reward)*(betDetails.amount))/100 ;
                betDetails.changeClaimedStatus();
            }
        }
        
        TransferHelper.safeTransfer(address(XIV), msg.sender , amount);
        tokenStakedAmount -= amount;
    }

    function betFlexible(uint amount, address coinAddress, uint index, uint _dayIndex) external validateBetArguments(amount, coinAddress, uint(2)) {
        require(((index >= uint(0) ) && (index <= dropValueFlexible.length - 1)), "Invalid dropvalue argument");
        require(((planDaysInFlexible[_dayIndex] != uint(0) && (_dayIndex > uint(0)))), "Invalid day argument");
        saveBetDetails(msg.sender, amount, dropValueFlexible[index], rewardFlexible[index], planDaysInFlexible[_dayIndex], coinAddress,uint(2));
    }

    function betFixed(uint amount, address coinAddress) external validateBetArguments(amount, coinAddress, uint(1)) {     
        saveBetDetails(msg.sender, amount, dropValueFixed, rewardFixed, planDaysInFixed, coinAddress,uint(1));
    }

    function saveBetDetails(address user,uint amount, uint _dropValue, uint _reward, uint _days, address _coinAddress, uint _planType) internal {

        if(_days == 1){
            require((block.timestamp  >= referenceTime() + (DAYS_IN_SECONDS/uint(2))) && (block.timestamp  <= referenceTime() + 4 hours + (DAYS_IN_SECONDS/uint(2))), "Bet not open for 1 day");
        }else if(_days == 3){
            require((block.timestamp  >= referenceTime() + (DAYS_IN_SECONDS/uint(2))) && (block.timestamp <= referenceTime() + 12 hours + (DAYS_IN_SECONDS/uint(2))), "Bet not open for 3 day");

        }

        betCounter[user]++;
        TransferHelper.safeTransferFrom(address(XIV), user, address(this), amount);
        BettingLibrary.BettingDetails storage betDetails = userBets[user][betCounter[msg.sender]];
        isEligibleForBet[user][_planType][_coinAddress] = true;   
       // uint initialPrice = 
        betDetails.setBetDetails(amount, uint(0), _reward, _dropValue, (referenceTime() + (_days * DAYS_IN_SECONDS) + (DAYS_IN_SECONDS/uint(2))), _coinAddress, _planType);

    }

    function addCoins (uint _coinType, uint planType, address coinAddress) external  onlyOwnerAccess {
        require((_coinType == 1 || _coinType == 2 || _coinType == 3), "Invalid CoinType");
        require((planType == 1 || planType == 2), "Invalid PlanType");
        require(coinAddress != address(0), "Address cant be zero");
        uint counter;

        if(_coinType == 1){
            counter =  defiCoinsCounter;
            defiCoinsCounter++;
        }else if(_coinType == 2){
            counter =  chainCoinsCounter;
            chainCoinsCounter++;
        }else {
            counter =  NFTCoinsCounter;
            NFTCoinsCounter++;
        }

        coins[_coinType][planType][counter] = coinAddress;
        changeCoinStaus(coinAddress,true);  
        emit Addcoins(_coinType, planType, counter, true, coinAddress); 
    }

    function changeCoinStaus (address coinAddress, bool status) public onlyOwnerAccess {
        coinStatus[coinAddress] = status;
        emit CoinStatus(coinAddress,status);
    }

    function stakeTokens(uint128 amount) external  checkAmount(amount,miniStakeAmount, true, msg.sender)  nonReentrant {
        TransferHelper.safeTransferFrom(address(XIV), msg.sender, address(this), amount);
        stake[msg.sender].stakedTokens += amount;
        stake[msg.sender].stakedTokensWithRewards += amount;
        tokenStakedAmount += amount;  
        
        if (stake[msg.sender].stakingTime  >  block.timestamp) { 
            stake[msg.sender].amountLocked += amount;
        } else {
            stake[msg.sender].stakingTime = uint64 (block.timestamp + 30 days);
            stake[msg.sender].amountLocked = amount;
        }
        
        emit LPEvent(0,msg.sender,amount,block.timestamp);
    }

    function unStakeTokens(uint128 amount) external checkAmount(amount, uint128(0), false, msg.sender) nonReentrant{
        stake[msg.sender].stakingTime  >  block.timestamp ? require(((stake[msg.sender].stakedTokensWithRewards - stake[msg.sender].amountLocked) >= amount), "Invalid Unstake") : require((stake[msg.sender].stakedTokensWithRewards >= amount), "Invalid Unstake");
        TransferHelper.safeTransferFrom(address(XIV), address(this), msg.sender, amount);
        tokenStakedAmount -= amount; 
        amount >= stake[msg.sender].stakedTokens ? stake[msg.sender].stakedTokens = uint128(0) : stake[msg.sender].stakedTokens -= amount;
        stake[msg.sender].stakedTokensWithRewards -= amount;
        emit LPEvent(1,msg.sender,amount,block.timestamp);
    }

    function updateminiStakeAmount (uint256 _miniStakeAmount ) external onlyOwnerAccess {
        miniStakeAmount  = _miniStakeAmount ;
    }

    function updateBetFactorLP(uint256 _betFactorLP) external onlyOwnerAccess{
        betFactorLP = _betFactorLP;
    }

    function updateMaxBetAmount(uint256 _maxBetAmount) external onlyOwnerAccess{
        maxBetAmount = _maxBetAmount;
    }

    function updateMinBetAmount(uint256 _miniBetAmount) external onlyOwnerAccess{
        miniBetAmount = _miniBetAmount;
    }

    function getFlexiblePalnDetails () external view returns (uint32 [] memory,uint32 [] memory ){
        return (dropValueFlexible, rewardFlexible);
    }

    function referenceTime () internal view returns(uint) {
        return ((block.timestamp/DAYS_IN_SECONDS)*(DAYS_IN_SECONDS));
    }
} 








