            tokenCount += amount;
        
            // Mint token
            uint256[] memory tokenIds = IERC721CreatorCore(_creator).mintExtensionBatch(to, amount);

            for (uint i = 0; i < tokenIds.length; i++) {
                emit Unveil(tokenStart+i, _creator, tokenIds[i]);
