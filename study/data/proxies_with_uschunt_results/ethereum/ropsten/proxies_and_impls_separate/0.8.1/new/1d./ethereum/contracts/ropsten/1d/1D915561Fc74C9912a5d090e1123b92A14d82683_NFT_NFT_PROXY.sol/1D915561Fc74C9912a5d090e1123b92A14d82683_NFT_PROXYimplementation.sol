  function airDropToAdresses(address[] calldata _users, uint256 _mintAmountPerAddress) public onlyOwner {
    require(!paused, "the contract is paused");
    require(_mintAmountPerAddress > 0, "need to mint at least 1 NFT");
    require((_users.length * _mintAmountPerAddress <= remainingAirDropSupply), "max air drop NFT limit exceeded");
    uint256 current_token_id = totalSupply() + 1;
    
    for (uint256 ui = 0; ui < _users.length; ui++) {
      for (uint256 i = 1; i <= _mintAmountPerAddress; i++) {
        _safeMint(_users[ui], current_token_id);
        current_token_id++;
      }
    }

    remainingAirDropSupply -= _users.length * _mintAmountPerAddress;
  }

  // whitelisted users will receive a signature of keccak256("\x19Ethereum Signed Message:\n20" + whitelisted_address) signed with owner's key.
  // since this function also checks msg.sender, this signature is not a private information and can only be used by whitelisted_address.
  // whitelisted addresses can call this function as many times as they can with single signature, but they cannot mint more than 
  //whitelistedNftPerAddressLimit using this function.
  function mintWhitelisted(uint256 _mintAmount, uint8 v, bytes32 r, bytes32 s) public payable {
    require(!paused, "the contract is paused");
    uint256 supply = totalSupply();
    require(_mintAmount > 0, "need to mint at least 1 NFT");
    require(_mintAmount <= maxMintAmount, "max mint amount per session exceeded");
    require((supply + _mintAmount + remainingAirDropSupply) <= maxSupply, "max NFT limit exceeded");
    
    bytes32 hash = keccak256(abi.encodePacked(eip191prefix, msg.sender));
    require(owner() == ecrecover(hash, v, r, s), "signature is wrong!!");

    uint256 userMintedCount = whitelistedAddressMintedTokenCount[msg.sender];
    require(userMintedCount + _mintAmount <= whitelistedNftPerAddressLimit, "max NFT per address exceeded");
    require(msg.value >= cost * _mintAmount, "insufficient funds");

    for (uint256 i = 1; i <= _mintAmount; i++) {
      whitelistedTokenID2Info[supply + i].addr = msg.sender;
      whitelistedTokenID2Info[supply + i].tokenIndex = uint32(userMintedCount + i);
      _safeMint(msg.sender, supply + i);
    }
    whitelistedAddressMintedTokenCount[msg.sender] += uint32(_mintAmount);
  }

  function walletOfOwner(address _owner_addr)
    public
    view
    returns (uint256[] memory)
  {
    uint256 ownerTokenCount = balanceOf(_owner_addr);
    uint256[] memory tokenIds = new uint256[](ownerTokenCount);
    for (uint256 i; i < ownerTokenCount; i++) {
      tokenIds[i] = tokenOfOwnerByIndex(_owner_addr, i);
    }
    return tokenIds;
  }

  function tokenURI(uint256 tokenId)
    public
    view
    virtual
    override
    returns (string memory)
  {
    require(
      _exists(tokenId),
      "ERC721Metadata: URI query for nonexistent token"
    );
    
    if(revealed == false) {
      if (revealWhitelistASAP) {
        WhitelistInfo memory token_info = whitelistedTokenID2Info[tokenId];
        if (token_info.addr != address(0x0)) {
          string memory currentWhitelistedBaseURI = _whitelistedBaseURI();
          return bytes(currentWhitelistedBaseURI).length > 0
            ? string(abi.encodePacked(currentWhitelistedBaseURI, uint256(uint160(token_info.addr)).toString(), URISeperator, uint256(token_info.tokenIndex).toString(), baseExtension))
            : "";
        }
      }
      return notRevealedUri;
    }

    string memory currentBaseURI = _baseURI();
    return bytes(currentBaseURI).length > 0
        ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), baseExtension))
        : "";
  }

