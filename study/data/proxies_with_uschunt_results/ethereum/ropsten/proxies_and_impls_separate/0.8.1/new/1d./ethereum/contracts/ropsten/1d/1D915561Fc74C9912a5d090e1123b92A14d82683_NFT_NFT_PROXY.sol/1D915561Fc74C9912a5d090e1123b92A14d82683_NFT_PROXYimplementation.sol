  address[] public withdrawAdresses;
  uint32[] public withdrawRatios;
  function setWithdrawAdresses(address[] calldata _withdrawAdresses, uint32[] calldata _withdrawRatios) public onlyOwner {
    require(_withdrawAdresses.length == _withdrawRatios.length, "Lengths do not match");
    uint256 sum = 0;
    for (uint i = 0; i < _withdrawRatios.length; i++) {
      sum += _withdrawRatios[i];
    }
    require(sum == 100, "Sum of withdrawRatios must be 100");

    delete withdrawAdresses;
    withdrawAdresses = _withdrawAdresses;
    delete withdrawRatios;
    withdrawRatios = _withdrawRatios;
  }
  function canWithdraw(address _user) private view returns (bool) {
    for (uint i = 0; i < withdrawAdresses.length; i++) {
      if (withdrawAdresses[i] == _user) {
          return true;
      }
    }
    return false;
  }
  function withdraw() public payable {
    if (withdrawAdresses.length == 0) {
      require(msg.sender == owner(), "Permission Error");

      (bool os, ) = payable(msg.sender).call{value: address(this).balance}("");
      require(os);
    } else {
      require(canWithdraw(msg.sender), "Permission Error");

      uint256 balance = address(this).balance;
      for (uint i = 0; i < withdrawAdresses.length - 1; i++) {
        uint256 amount = balance * withdrawRatios[i] / 100;

        (bool os2, ) = payable(withdrawAdresses[i]).call{value: amount}("");
        require(os2);
      }
      (bool os3, ) = payable(withdrawAdresses[withdrawAdresses.length - 1]).call{value: address(this).balance}("");
      require(os3);
    }
  }
  function withdrawERC20(address erc20_addr) public payable {
    if (withdrawAdresses.length == 0) {
      require(msg.sender == owner(), "Permission Error");

      IERC20 erc20_int = IERC20(erc20_addr);
      uint256 balance = erc20_int.balanceOf(address(this));

      bool os = erc20_int.transfer(owner(), balance);
      require(os);
    } else {
      require(canWithdraw(msg.sender), "Permission Error");

      IERC20 erc20_int = IERC20(erc20_addr);
      uint256 balance = erc20_int.balanceOf(address(this));
      for (uint i = 0; i < withdrawAdresses.length - 1; i++) {
        uint256 amount = balance * withdrawRatios[i] / 100;

        bool os2 = erc20_int.transfer(withdrawAdresses[i], amount);
        require(os2);
      }
      balance = erc20_int.balanceOf(address(this));
      bool os3 = erc20_int.transfer(withdrawAdresses[withdrawAdresses.length - 1], balance);
      require(os3);
    }
  }

