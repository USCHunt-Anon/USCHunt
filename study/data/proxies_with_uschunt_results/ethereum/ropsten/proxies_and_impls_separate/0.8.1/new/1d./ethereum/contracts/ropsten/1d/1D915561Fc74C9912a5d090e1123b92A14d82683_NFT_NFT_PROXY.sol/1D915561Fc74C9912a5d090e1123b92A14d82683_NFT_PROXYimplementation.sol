    require(acceptERC20, "NFT does not accept ERC20");

    if (ERC20CostInEther) {
      uint256 mintCostInWei = ERC20Cost * _mintAmount;
      (uint160 sqrtPriceX96,,,,,, ) = uniswapERC20Pool.slot0();
      uint256 sqrtPriceX64 = sqrtPriceX96 >> 32;
      // TODO: check this part again!!
      /*uint256 priceERC20inWei = sqrtPriceX64.mul(sqrtPriceX64).mul(1e18) >> (64 * 2);
      uint256 mintCostInERC20 = mintCostInWei * 1e18 / priceERC20inWei;
      return mintCostInERC20;*/
      uint256 howMuchERC20isOneETH = sqrtPriceX64.mul(sqrtPriceX64).mul(1e18) >> (64 * 2);
      uint256 mintCostInERC20 = howMuchERC20isOneETH.mul(mintCostInWei) / 1e18;
      return mintCostInERC20;
    } else {
      return ERC20Cost;
    }
  }

  function getMintCostInERC20_2(uint256 _mintAmount)
    public
    view
    returns (uint256)
  {
    require(acceptERC20, "NFT does not accept ERC20");

    if (ERC20CostInEther) {
      uint256 mintCostInWei = ERC20Cost * _mintAmount;

      (uint256 Res0, uint256 Res1,) = uniswapERC20Pair.getReserves();

      // TODO: check this part again!!
      return ((mintCostInWei.mul(Res0))/Res1);
    } else {
      return ERC20Cost;
    }
  }

  function mintWithERC20(uint256 _mintAmount, uint256 _maxMintCost) public {
    require(acceptERC20, "NFT does not accept ERC20");

    require(!paused, "the contract is paused");
    require(_mintAmount > 0, "need to mint at least 1 NFT");
    require(_mintAmount <= maxMintAmount, "max mint amount per session exceeded");
    require(!onlyWhitelisted, "Use mintWhitelisted to mint");
    uint256 supply = totalSupply();
    require((supply + _mintAmount + remainingAirDropSupply) <= maxSupply, "max NFT limit exceeded");

    uint256 mintCostInERC20 = 0;
    if (use_second_erc20_price_function) {
      mintCostInERC20 = getMintCostInERC20_2(_mintAmount);
    } else {
      mintCostInERC20 = getMintCostInERC20(_mintAmount);
    }
    require(mintCostInERC20 <= _maxMintCost, "max mint cost exceeded");

    bool success = ERC20Token.transferFrom(msg.sender, address(this), mintCostInERC20);
    require(success);

    for (uint256 i = 1; i <= _mintAmount; i++) {
      _safeMint(msg.sender, supply + i);
    }
  }
