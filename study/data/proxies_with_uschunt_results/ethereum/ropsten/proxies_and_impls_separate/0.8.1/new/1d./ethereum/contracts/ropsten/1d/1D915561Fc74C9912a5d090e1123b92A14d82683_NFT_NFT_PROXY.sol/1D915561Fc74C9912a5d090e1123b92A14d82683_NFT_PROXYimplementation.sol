  // CREATORCORE
  function getRoyalties(uint256) external view returns (address payable[] memory recipients, uint256[] memory bps) { 
    recipients = new address payable[](1);
    recipients[0] = payable(address(this));
    bps = new uint256[](1);
    bps[0] = _royaltyBps;
    
    return (recipients, bps);
  }

