abstract contract XAssetStorageV1 is ERC20PermitUpgradeable, OwnableUpgradeable, BaseRelayRecipient, IXAsset {
    /// @dev minting delay
    uint256 public mintDelay;

    /// @dev pendingMint amount => address => timestamp
    mapping(uint256 => mapping(address => uint256)) internal pendingMint;

    function _msgData() internal view override(BaseRelayRecipient, ContextUpgradeable) returns (bytes calldata) {
        return BaseRelayRecipient._msgData();
    }

    function _msgSender() internal view override(BaseRelayRecipient, ContextUpgradeable) returns (address) {
        return BaseRelayRecipient._msgSender();
    }
}

contract XAsset is XAssetStorageV1 {
    /// @dev minimum mint delay period
    uint256 internal constant MINIIMUM_MINT_DELAY = 1 hours;

    /// @dev maximum mint delay period
    uint256 internal constant MAXIMUM_MINT_DELAY = 1 days;

    /// @dev vault
    IXVault public immutable vault;

    constructor(IXVault _vault) {
        vault = _vault;
    }

    /// @dev initialize
    /// @param _name ERC20 token name
    /// @param _symbol ERC20 token symbol
    function initialize(
        string memory _name,
        string memory _symbol,
        uint256 _delay
    ) external initializer {
        __Ownable_init();
        __ERC20Permit_init(_name);
        __ERC20_init(_name, _symbol);

        mintDelay = _delay;
    }

    /// @dev start minting of tokens
    /// @param to address to mint to
    /// @param amount amount to mint
    function startMint(address to, uint256 amount) external onlyOwner {
        pendingMint[amount][to] = block.timestamp;

        emit PendingMint(to, amount);
    }

    /// @dev set mint delay
    /// @param delay new mint delay period
    function setMintDelay(uint256 delay) external onlyOwner {
        require(delay >= MINIIMUM_MINT_DELAY, "INVALID_MINIMUM");
        require(delay <= MAXIMUM_MINT_DELAY, "INVALID_MAXIMUM");

        mintDelay = delay;

        emit MintDelayUpdated(delay, block.timestamp);
    }

    /// @dev mint
    /// @param to address to mint the tokens to
    /// @param amount amount of tokens to mint
    function mint(address to, uint256 amount) public onlyOwner {
        require(block.timestamp > pendingMint[amount][to] + mintDelay, "PENDING_MINT");

        delete pendingMint[amount][to];

        _mint(to, amount);
    }

    /// @dev mint to vault
    /// @param to address to mint to on vault
    /// @param amount of tokens to mint
    function mintToVault(address to, uint256 amount) external onlyOwner {
        mint(address(vault), amount);

        vault.deposit(IERC20(address(this)), address(vault), to, amount);
    }

    /// @dev burn
    /// @param amount amount of tokens to burn
    function burn(uint256 amount) external onlyOwner {
        _burn(_msgSender(), amount);
    }

    function isTrustedForwarder(address forwarder) public view override returns (bool) {}

    function versionRecipient() external view override returns (string memory) {}
}


